    <td  width="40%" valign="middle" class="campos"><div align="right">Valor Asignado: &nbsp;&nbsp;</div></td>
    <td  width="60%" valign="middle" class="camposDesc"><%=camponumerico(@subsidio.valor) rescue nil%></td>
  </tr>
  <tr>
    <td valign="middle" class="campos"><div align="right">Valor Utilizado:&nbsp;&nbsp; </div></td>
    <td valign="middle" class="camposDesc"><%=camponumerico(@subsidio.valor_utilizado) rescue nil%></td>
  </tr>
 <tr class="odd1">
    <td valign="middle" class="campos"><div align="right">Bolsa:&nbsp;&nbsp; </div></td>
    <td valign="middle" class="camposDesc"><%= @subsidio.bolsa%></td>
  </tr>
  <tr>
    <td valign="middle" class="campos"><div align="right">Nro Resolucion &oacute; Asignacion:&nbsp;&nbsp; </div></td>
    <td valign="middle" class="camposDesc"><%= @subsidio.resolucion%></td>
  </tr>
  <tr class="odd1">
    <td valign="middle" class="campos"><div align="right">Fecha Asignacion: &nbsp;&nbsp;</div></td>
    <td valign="middle" class="camposDesc"><%= @subsidio.fecha_asig%></td>
  </tr>
  <tr>
    <td valign="middle" class="campos"><div align="right">Entidad Otorga: &nbsp;&nbsp;</div></td>
