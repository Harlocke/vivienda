   <%= stylesheet_link_tag 'scaffold'%>
<div align="center">
<table width="550" border="0" cellpadding="2" cellspacing="2">
  <tr>
    <td height="30"><div class="col_enc">INFORMACION - <%=@subsidio.tipos_subsidio.descripcion %></div></td>
  </tr>
</table>
<table width="550" id="one-column-emphasis">
  <tr>
    <td colspan="2" height="20" class="oce-first"><div align="center">GRUPO FAMILIAR DE: <%=@subsidio.persona.autobuscar rescue nil%></div></td>
  </tr>
