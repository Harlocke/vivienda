<h1>Listing familiares</h1>

<table>
  <tr>
    <th>Descripcion</th>
  </tr>

<% @familiares.each do |familiar| %>
  <tr>
    <td><%=h familiar.descripcion %></td>
    <td><%= link_to 'Show', familiar %></td>
    <td><%= link_to 'Edit', edit_familiar_path(familiar) %></td>
    <td><%= link_to 'Destroy', familiar, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New familiar', new_familiar_path %>