para<table width="100%" border="0" cellpadding="0" cellspacing="0">
  <tr>
    <td><div align="center"><%=image_tag("frame.png")%></div></td>
  </tr>
</table>
<% if @existecontrato %>
<br/>
  <table width="100%" height="100%" border="0" cellpadding="0" cellspacing="0"  align="center">
    <tr>
      <td width="3%"><img src="/images/empleadosmin.png" width="64" height="64"/></td>
      <td width="97%"><div class="col_enc">&nbsp;&nbsp;INFORMACION VENCIMIENTO CONTRAROS</div></td>
    </tr>
  </table>
<br/>
  <table width=90%" border="0" align="center" cellpadding="0" cellspacing="0">
     <tr valign="middle">
        <td width="15%" height="30" class="oce-first"><div align="center">Nro Contrato</div></td>
        <td width="55%" height="30" class="oce-first">Contratista</td>
        <td width="10%" height="30" class="oce-first"><div align="center">Fecha Inicio</div></td>
        <td width="10%" height="30" class="oce-first"><div align="center">Fecha Final</div></td>
        <td width="15%" height="30" class="oce-first"><div align="center">Dias Venc.</div></td>
     </tr>
     <% @contratos.each do |contrato| %>
     <tr valign="middle">
        <td class="camposDesc"><div align="center"><%=contrato.nro_contrato rescue nil%></div></td>
        <td class="camposDesc"><%=contrato.identificacion%> - <%=contrato.nombre%></td>
        <td class="camposDesc"><div align="center"><%=contrato.fecha_inicio%></div></td>
        <td class="camposDesc"><div align="center"><%=contrato.fch%></div></td>
        <td class="camposDesc"><div align="center"><%=contrato.dias%></div></td>
      </tr>
      <% end %>
  </table>
<% end %>