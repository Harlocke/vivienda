<table width="100%" height="40">
      <tr>
        <td>
          <table width="100%" height="100%" border="0" cellpadding="0" cellspacing="0">
            <tr>
              <td width="3%"><img src="/images/inventariotecnologiamin.png" width="64" height="64"/></td>
              <td width="94%"><div class="col_enc">&nbsp;&nbsp;INFORMACION IMAGENES MANTENIMIENTO ( <%=@impresorasreporte.tipo rescue nil%>)</div></td>
              <td width="3%"><div align="right"><div class="uiButton"><img class="img" src="/images/1atras.png"/>&nbsp;&nbsp;<span class="uiButtonText"><%= link_to 'Regresar',  edit_impresora_path(@impresorasreporte.impresora_id) %></span></div></div>            </td>
            </tr>
          </table></td>
      </tr>
</table>
<table width="80%" border="0" align="center" cellpadding="0" cellspacing="0">
  <tr valign="middle">
    <td width="10%" height="30" class="oce-first">Tipo Mantenimiento</td>
    <td width="10%" height="30" class="oce-first">Fecha</td>
    <td width="80%" height="30" class="oce-first">Descripcion Mantenimiento</td>
  </tr>
   <tr>
     <td height="30" class="camposDesc"><%=@impresorasreporte.tipo rescue nil%></td>
     <td height="30" class="camposDesc"><%=@impresorasreporte.fecha_reporte rescue nil%></td>
     <td height="30" class="camposDesc"><%=@impresorasreporte.descripcion rescue nil%></td>
 </tr>
</table>
   <br/>
<table width="80%" height="40" align="center" >
      <tr>
        <td>
          <table width="100%" height="100%" border="0" cellpadding="0" cellspacing="0">
            <tr>
              <td width="94%"><div class="col_enc">&nbsp;&nbsp;IMAGENES</div></td>
              <td width="3%"><div align="right">
                  <% if permiso("impresorasimagen","C").to_s == "S" %>
                   <div class="uiButton"><img class="img" src="/images/1nuevo.png"/>&nbsp;&nbsp;<span class="uiButtonText"><%= link_to "Nuevo", new_img_impresorasimagen_path %></span></div>
                 <% end %></div></td>
            </tr>
          </table></td>
      </tr>
</table>
<table width="80%" border="0" align="center" cellspacing="1" cellpadding="1">
  <tr>
    <td class="oce-first" width="10%">Fecha</td>
    <td class="oce-first" width="35%">Descripcion</td>
    <td class="oce-first" width="40%">Nombre Archivo</td>
    <td class="oce-first" width="15%">&nbsp;</td>
  </tr>
</table>
<% @impresorasimagenes.each do |impresorasimagen| %>
<table width="80%" border="0" cellspacing="0" cellpadding="0" align="center" >
  <tr class="<%=cycle('even1', 'odd1')%>">
    <td width="10%" class="camposDesc"><%=impresorasimagen.created_at%></td>
    <td width="35%" class="camposDesc"><%=impresorasimagen.descripcion rescue nil%></td>
    <td width="40%" class="camposDesc"><%=impresorasimagen.impresora_file_name rescue nil%></td>
    <td width="15%" class="camposDesc"><div align="center">
         <%= link_to "Ver", impresorasimagen.impresora.url(:original, false),
             { :popup => ['new_window','height=450,width=600,scrollbars=yes'], :class=>"verlink"}%>
        <% if permiso("impresorasimagen","E").to_s == "S" %>&nbsp;|
       <%= link_to 'Borrar', img_impresorasimagen_path(@impresorasreporte, impresorasimagen),
                :confirm => 'Esta Seguro de Eliminar?', :method => :delete, :class=>"borrarlink"%>
       <%end%>
</div></td>
  </tr>
</table>
<% end %>
<br />
