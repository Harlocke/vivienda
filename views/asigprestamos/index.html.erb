<table width="100%" height="40">
      <tr>
        <td>
          <table width="100%" height="100%" border="0" cellpadding="0" cellspacing="0">
            <tr>
              <td width="3%"><img src="/images/titulacionmin.png" width="64" height="64"/></td>
              <td width="94%"><div class="col_enc">PROCESO DE PRESTAMOS PASIVOS</div></td>
              <td width="3%"><div align="right"><%= link_to image_tag('atras.png',
	            { :border=>0, :title => "Listado de Predios"}),titulaciones_path %></div></td>
            </tr>
          </table></td>
      </tr>
</table>
<div align="center">
<table width="90%" height="40" border="0" cellpadding="0" cellspacing="0">
   <tr>
    <td>&nbsp;
     <% form_remote_tag :url => {:action => 'add_asigprestamo'}, :complete => "asigprestamo_cobama.value=nil"  do %>
      <div align="center">
        <table width="30%" border="0" cellspacing="2" cellpadding="0">
          <tr>
            <td width="70%"><div class="campos">Nro del Cobama</div></td>
            <td width="30%"><div class="campos"></div></td>
          </tr>
          <tr>
            <td width="10%"><%= text_field 'asigprestamo', 'cobama',{ :class => "campo",:size => 25,
                  :onFocus => "this.style.border='1px solid #f00'",
                  :onBlur => "this.style.border='1px solid #bbb'"
                   }  %></td>            
            <td width="20%"><div align="right">
              <div class="uiButton"><img class="img" src="/images/guardar.png"/>&nbsp;&nbsp;<span class="uiButtonText"><%= submit_tag "Guardar", :disable_with => "Guardando..."%></span></div></div></td>
          </tr>
          
        </table>
      </div>
      <table width="100%" height="0" border="0" cellpadding="0" cellspacing="0">
        <tr>
          <td height="10"><hr size="1" noshade class="hrlinea"/></td>
        </tr>
      </table>
     <% end %></td>
  </tr>
</table>

<br />
<div id="asigprestamo_list">
<%= render :partial => 'list' %>
</div>
<br/>
</div>

