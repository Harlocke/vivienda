      <tr>
<%
          cantviviendas    = Vivienda.count(:conditions => [' proyecto_id = ? ', "#{@proyectosficha.proyecto_id}"])
          cantentregadas   = Vivienda.count(:conditions => [' proyecto_id = ? and entregado = ?' , "#{@proyectosficha.proyecto_id}", "S"])
          cantnoentregadas = cantviviendas - cantentregadas
          cantocupadas     = Vivienda.count(:conditions => [' proyecto_id = ? and estado = ?' , "#{@proyectosficha.proyecto_id}", "O"])
          cantdisponibles  = Vivienda.count(:conditions => [' proyecto_id = ? and estado = ?' , "#{@proyectosficha.proyecto_id}", "D"])
          cantescr         = Viviendastramite.count(:conditions => [' viviendastipostramite_id = 10086 and vivienda_id in (select id from viviendas where proyecto_id = ?)' , "#{@proyectosficha.proyecto_id}"])
          cantbloques      = Bloque.count(:conditions => [' proyecto_id = ?' , "#{@proyectosficha.proyecto_id}"])
%>
        <td class="camposDesc"><div align="center"><%=cantviviendas%></div></td>
        <td class="camposDesc"><div align="center"><%=cantbloques%></div></td>
        <td class="camposDesc"><div align="center"><%=cantentregadas%></div></td>
        <td class="camposDesc"><div align="center"><%=cantnoentregadas%></div></td>
        <td class="camposDesc"><div align="center"><%=cantocupadas%></div></td>
        <td class="camposDesc"><div align="center"><%=cantdisponibles%></div></td>
        <td class="camposDesc"><div align="center"><%=cantescr%></div></td>
