<table width="800" border="0" cellpadding="0" cellspacing="1" class="sample">
      <tr class="odd1">
        <td width="45%" class="campos">ETAPA</td>
        <td width="45%" class="campos">SUBETAPA</td>
        <td width="10%" class="campos"><div align="center">CANTIDAD</div></td>
      </tr>
<%

        objetos = Objeto.find_by_sql("select distinct tt.etapa, tt.descripcion, count(9) cant
                                     from   viviendastramites t, viviendas v, viviendastipostramites tt
                                     where  v.proyecto_id = 1
                                     and    v.id = t.vivienda_id
                                     and    t.VIVIENDASTIPOSTRAMITE_ID = tt.id
                                     and    tt.estado = 'A'
                                     and    t.fecha_esperada is null
                                     group by tt.etapa,tt.descripcion
                                     order by tt.etapa,tt.descripcion")
        objetos.each do |objeto|
%>
      <tr>
        <td class="camposDesc"><%=objeto.etapa%></td>
        <td class="camposDesc"><%=objeto.descripcion%></td>
        <td class="camposDesc"><div align="center"><%=objeto.cant%></div></td>
      </tr>
<%
        end
%>
