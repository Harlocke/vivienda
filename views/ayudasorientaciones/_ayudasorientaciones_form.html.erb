<table width="95%"  border="0" cellpadding="0" cellspacing="0">
  <tr>
    <td>
      <table width="100%" border="0" cellpadding="0" cellspacing="2">
        <tr class="odd1">
          <td width="17%" class="campos">Notificación telefónica</td>
          <td width="16%" class="campos">Fecha notificación</td>
          <td width="18%" class="campos">Renotificación telefónica</td>
          <td width="16%" class="campos">Fecha renotificación</td>
          <td width="17%" class="campos">Notificación individual</td>
          <td width="16%" class="campos">Fecha notificación</td>
        </tr>
        <tr>
          <td><%= select(:ayudasorientacion, :not_telefonica, select_sino,{ :include_blank => true },
                        { :style=>"width:50px", :class => "campo", :onFocus => "this.style.border='1px solid #f00'", :onBlur => "this.style.border='1px solid #bbb'" })%>
              <%= error_message_on :ayudasorientacion, :not_telefonica, :css_class =>"cerror"%></td>
          <td><%= calendar_date_select "ayudasorientacion", "fecha_not_telefonica", :year_range => select_rangocalendar, :class=> "campo", :placeholder=>"AAAA-MM-DD", :size => 10, :time => false%>
              <%= error_message_on :ayudasorientacion, :fecha_not_telefonica, :css_class =>"cerror"%></td>
          <td><%= select(:ayudasorientacion, :renot_telefonica, select_sino,{ :include_blank => true },
                        { :style=>"width:50px", :class => "campo", :onFocus => "this.style.border='1px solid #f00'", :onBlur => "this.style.border='1px solid #bbb'" })%>
              <%= error_message_on :ayudasorientacion, :renot_telefonica, :css_class =>"cerror"%></td>
          <td><%= calendar_date_select "ayudasorientacion", "fecha_renot_telefonica", :year_range => select_rangocalendar, :class=> "campo", :placeholder=>"AAAA-MM-DD", :size => 10, :time => false%>
              <%= error_message_on :ayudasorientacion, :fecha_renot_telefonica, :css_class =>"cerror"%></td>
          <td><%= select(:ayudasorientacion, :not_individual, select_sino,{ :include_blank => true },
                        { :style=>"width:50px", :class => "campo", :onFocus => "this.style.border='1px solid #f00'", :onBlur => "this.style.border='1px solid #bbb'" })%>
              <%= error_message_on :ayudasorientacion, :not_individual, :css_class =>"cerror"%></td>
          <td><%= calendar_date_select "ayudasorientacion", "fecha_not_individual", :year_range => select_rangocalendar, :class=> "campo", :placeholder=>"AAAA-MM-DD", :size => 10, :time => false%>
              <%= error_message_on :ayudasorientacion, :fecha_not_individual, :css_class =>"cerror"%></td>
        </tr>
      </table>
      <table width="100%" border="0" cellpadding="0" cellspacing="2">
        <tr class="odd1">
          <td width="17%" class="campos">Renotificación individual</td>
          <td width="16%" class="campos">Fecha renotificación</td>
          <td width="16%" class="campos">Acta de no aceptación</td>
          <td width="18%" class="campos">Fecha acta de no aceptación</td>
          <td width="17%" class="campos">Notificación grupal</td>
          <td width="16%" class="campos">Fecha notificación</td>
        </tr>
        <tr>
          <td><%= select(:ayudasorientacion, :renot_individual, select_sino,{ :include_blank => true },
                        { :style=>"width:50px", :class => "campo", :onFocus => "this.style.border='1px solid #f00'", :onBlur => "this.style.border='1px solid #bbb'" })%>
              <%= error_message_on :ayudasorientacion, :renot_individual, :css_class =>"cerror"%></td>
          <td><%= calendar_date_select "ayudasorientacion", "fecha_renot_individual", :year_range => select_rangocalendar, :class=> "campo", :placeholder=>"AAAA-MM-DD", :size => 10, :time => false%>
              <%= error_message_on :ayudasorientacion, :fecha_renot_individual, :css_class =>"cerror"%></td>
          <td><%= select(:ayudasorientacion, :acta, select_sino,{ :include_blank => true },
                        { :style=>"width:50px", :class => "campo", :onFocus => "this.style.border='1px solid #f00'", :onBlur => "this.style.border='1px solid #bbb'" })%>
              <%= error_message_on :ayudasorientacion, :acta, :css_class =>"cerror"%></td>
          <td><%= calendar_date_select "ayudasorientacion", "fecha_acta", :year_range => select_rangocalendar, :class=> "campo", :placeholder=>"AAAA-MM-DD", :size => 10, :time => false%>
              <%= error_message_on :ayudasorientacion, :fecha_acta, :css_class =>"cerror"%></td>
          <td><%= select(:ayudasorientacion, :not_grupal, select_sino,{ :include_blank => true },
                        { :style=>"width:50px", :class => "campo", :onFocus => "this.style.border='1px solid #f00'", :onBlur => "this.style.border='1px solid #bbb'" })%>
              <%= error_message_on :ayudasorientacion, :not_grupal, :css_class =>"cerror"%></td>
          <td><%= calendar_date_select "ayudasorientacion", "fecha_not_grupal", :year_range => select_rangocalendar, :class=> "campo", :placeholder=>"AAAA-MM-DD", :size => 10, :time => false%>
              <%= error_message_on :ayudasorientacion, :fecha_not_grupal, :css_class =>"cerror"%></td>
        </tr>
      </table>
      <table width="100%" border="0" cellpadding="0" cellspacing="2">
        <tr class="odd1">
          <td width="17%" class="campos">Renotificación grupal</td>
          <td width="16%" class="campos">Fecha renotificación</td>
          <td width="16%" class="campos">Notificacion Carta</td>
          <td width="18%" class="campos">Fecha notificación</td>
          <td width="17%" class="campos">Notificacion edicto</td>
          <td width="16%" class="campos">Fecha notificación</td>
        </tr>
        <tr>
          <td><%= select(:ayudasorientacion, :renot_grupal, select_sino,{ :include_blank => true },
                        { :style=>"width:50px", :class => "campo", :onFocus => "this.style.border='1px solid #f00'", :onBlur => "this.style.border='1px solid #bbb'" })%>
              <%= error_message_on :ayudasorientacion, :renot_grupal, :css_class =>"cerror"%></td>
          <td><%= calendar_date_select "ayudasorientacion", "fecha_renot_grupal", :year_range => select_rangocalendar, :class=> "campo", :placeholder=>"AAAA-MM-DD", :size => 10, :time => false%>
              <%= error_message_on :ayudasorientacion, :fecha_renot_grupal, :css_class =>"cerror"%></td>
          <td><%= select(:ayudasorientacion, :not_carta, select_sino,{ :include_blank => true },
                        { :style=>"width:50px", :class => "campo", :onFocus => "this.style.border='1px solid #f00'", :onBlur => "this.style.border='1px solid #bbb'" })%>
              <%= error_message_on :ayudasorientacion, :not_carta, :css_class =>"cerror"%></td>
          <td><%= calendar_date_select "ayudasorientacion", "fecha_not_carta", :year_range => select_rangocalendar, :class=> "campo", :placeholder=>"AAAA-MM-DD", :size => 10, :time => false%>
              <%= error_message_on :ayudasorientacion, :fecha_not_carta, :css_class =>"cerror"%></td>
          <td><%= select(:ayudasorientacion, :not_edicto, select_sino,{ :include_blank => true },
                        { :style=>"width:50px", :class => "campo", :onFocus => "this.style.border='1px solid #f00'", :onBlur => "this.style.border='1px solid #bbb'" })%>
              <%= error_message_on :ayudasorientacion, :not_edicto, :css_class =>"cerror"%></td>
          <td><%= calendar_date_select "ayudasorientacion", "fecha_not_edicto", :year_range => select_rangocalendar, :class=> "campo", :placeholder=>"AAAA-MM-DD", :size => 10, :time => false%>
              <%= error_message_on :ayudasorientacion, :fecha_not_edicto, :css_class =>"cerror"%></td>
        </tr>
      </table>
      <table width="100%" border="0" cellpadding="0" cellspacing="2">
        <tr class="odd1">
          <td width="20%" class="campos">Carta devuelta</td>
          <td width="80%" class="campos">Motivo devolución</td>
        </tr>
        <tr>
          <td><%= select(:ayudasorientacion, :carta_dev, select_sino,{ :include_blank => true },
                        { :style=>"width:50px", :class => "campo", :onFocus => "this.style.border='1px solid #f00'", :onBlur => "this.style.border='1px solid #bbb'" })%>
              <%= error_message_on :ayudasorientacion, :carta_dev, :css_class =>"cerror"%></td>
          <td><%= text_field :ayudasorientacion, :motivo_dev, { :class => "campo", :size => 65,
                      :onChange=> "javascript:this.value=this.value.toUpperCase();",
                      :onFocus => "this.style.border='1px solid #f00'",
                      :onBlur => "this.style.border='1px solid #bbb'" }%>
            <%= error_message_on :ayudasorientacion, :motivo_dev, :css_class =>"cerror"%></td>
        </tr>
      </table>
      <table width="100%" height="0" border="0" cellpadding="0" cellspacing="0">
        <tr>
         <td height="10"><hr size="1" noshade class="hrlinea"/></td>
        </tr>
      </table>
      <table width="100%" border="0" cellpadding="2" cellspacing="2">
        <tr>
          <td  height="30">
            <div align="right">
              <div class="uiButton"><img class="img" src="/images/guardar.png"/>&nbsp;&nbsp;<span class="uiButtonText"><%= hidden_field :ayudasorientacion, :id %><%= submit_tag "Guardar", :disable_with => "Guardando..."%></span></div>
            </div>
          </td>
        </tr>
      </table>
    </td>
  </tr>
</table>