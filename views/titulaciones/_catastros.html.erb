<% if @titulacion.catastro_id.to_i > 0 and @existecatastrodatos %>
   <fieldset>
      <div align="left" style="color:red;" class="camposDescN16">BENEFICIARIOS PARA VALIDAR</div><br/>
      <div align="center">
      <table width="100%" border="0" align="center" cellpadding="0" cellspacing="0">
        <tr valign="middle">
          <td width="12%" height="30" align="left" class="oce-first">Matricula</td>
          <td width="30%" class="oce-first">Direccion</td>
          <td width="8%" class="oce-first">Area Lote</td>
          <td width="10%" class="oce-first">Documento</td>
          <td width="35%" class="oce-first">Nombre</td>
          <td width="5%" class="oce-first"></td>
        </tr>
      <% @catastrosdatos.each do |catastro| %>
         <tr  valign="middle" class="<%=cycle('even1', 'odd1')%>" id="titulacionespersona_<%=catastro.id%>">
           <td height="30" align="left" class="camposDesc"><%=catastro.matricula rescue nil%></td>
           <td align="left" class="camposDesc"><%=catastro.direccion rescue nil%></td>
           <td align="left" class="camposDesc"><%=catastro.arealote rescue nil%></td>
           <td align="left" class="camposDesc"><%=catastro.documento rescue nil%></td>
           <td align="left" class="camposDesc"><%=catastro.nombre rescue nil%></td>
           <td class="camposDesc"><div align="center">
               <% if catastro.cargado.to_s == ""
                   if permiso("titulacion","A").to_s == "S" %>
                    <%=link_to 'Cargue', {:controller=>"titulacionescargues", :action=>"new", :id=>catastro.id, :titulacion_id=>@titulacion.id}, :confirm=>"Seguro que desea iniciar el proceso de incluir el registro seleccionado?",:class=>"aplazalink", :title =>"Cargar registro"%>
                   <% end %>
               <% else %>
                  <img src="/images/connect.png" title="Ya esta cargado"/>
               <% end %></div></td>
         </tr>
      <% end%>
      </table>
      </div>
   </fieldset>
 <% end %>
