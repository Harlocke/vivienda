<% form_for(@titulacion) do |f| %>
 <script  type="text/javascript">
      Event.observe(window, 'load', function() {
        <%
        @status = 'true'
        @status2 = 'true'
        if @titulacion.acto_pot.to_s == 'SI'
          @status = 'false'
        end
        if @titulacion.embargo.to_s == 'SI'
          @status2 = 'false'
        end
        %>
         document.getElementById("titulacion_embargo").disabled = <%=@status%>
         document.getElementById("titulacion_rph").disabled = <%=@status2%>
         document.getElementById("titulacion_area_vur").disabled = <%=@status2%>   
        });

       function habcampo(f)
        {   
          if (f.titulacion_acto_pot.value == "SI"){
            f.titulacion_embargo.disabled = false;
          } else if (f.titulacion_acto_pot.value == "NO"){
            f.titulacion_embargo.disabled = true;
          }
          if (f.titulacion_embargo.value == "SI"){
            f.titulacion_rph.disabled = true;
            f.titulacion_area_vur.disabled = true;
          } else if (f.titulacion_embargo.value == "NO"){
            f.titulacion_rph.disabled = false;
            f.titulacion_area_vur.disabled = false;
          }
          if (f.titulacion_rph.value == "SI"){
            f.titulacion_area_vur.disabled = true;
          } else if (f.titulacion_rph.value == "NO"){
            f.titulacion_area_vur.disabled = false;
          }
          if (f.titulacion_viabilidad.value == "SI"){
            f.titulacion_negado.disabled = true;
            f.titulacion_desistido.disabled = true;
           } else if (f.titulacion_viabilidad.value == "NO"){
            f.titulacion_negado.disabled = false;
            f.titulacion_desistido.disabled = false;
           } 
           if (f.titulacion_negado.value == "SI"){
            f.titulacion_desistido.disabled = true;
            f.titulacion_viabilidad.disabled = true;
           } else if (f.titulacion_negado.value == "NO"){
            f.titulacion_desistido.disabled = false;
            f.titulacion_viabilidad.disabled = false;
           } 
           if (f.titulacion_desistido.value == "SI"){
            f.titulacion_negado.disabled = true;
            f.titulacion_viabilidad.disabled = true;
           } else if (f.titulacion_desistido.value == "NO"){
            f.titulacion_negado.disabled = false;
            f.titulacion_viabilidad.disabled = false;
           } 
        }

    </script>
