<table width="100%" height="40">
      <tr>
        <td>
          <table width="100%" height="100%" border="0" cellpadding="0" cellspacing="0">
            <tr>
              <td width="3%"><img src="#" width="64" height="64"/></td>
              <td width="94%"><div class="col_enc">&nbsp;&nbsp;INFORMACION DEL CONTRATO ( <%=@fiduciascontrato.fiducia.nombre %> - <%= @fiduciascontrato.fiducia.nit%>)</div></td>
              <td width="3%"><div align="right"><div class="uiButton"><img class="img" src="/images/1atras.png"/>&nbsp;&nbsp;<span class="uiButtonText"><%= link_to 'Regresar',  edit_fiducia_path(@fiduciascontrato.fiducia_id) %></span></div></div>            </td>
            </tr>
          </table></td>
      </tr>
</table>
<table width="80%" border="0" align="center" cellpadding="0" cellspacing="0">
  <tr valign="middle">
    <td width="10%" height="30" class="oce-first">Nro Contrato</td>
    <td width="10%" height="30" class="oce-first">Fecha Inicio</td>
    <td width="10%" height="30" class="oce-first">Fecha Final</td>
    <td width="70%" height="30" class="oce-first">Proyecto</td>
  </tr>
   <tr>
     <td height="30" class="camposDesc"><%=@fiduciascontrato.nro_contrato%></td>
     <td height="30" class="camposDesc"><%=@fiduciascontrato.fecha_inicio%></td>
     <td height="30" class="camposDesc"><%=@fiduciascontrato.fecha_fin%></td>
     <td height="30" class="camposDesc"><%=@fiduciascontrato.proyecto%></td>
 </tr>
</table>
   <br>
<table width="80%" height="40" align="center" >
      <tr>
        <td>
          <table width="100%" height="100%" border="0" cellpadding="0" cellspacing="0">
            <tr>
              <td width="94%"><div class="col_enc">&nbsp;&nbsp;MODIFICACIONES</div></td>
              <td width="3%"><div align="right">
                  <%# if permiso("fiduciasconmodificacion","C").to_s == "S" %>
                  <div class="uiButton"><img class="img" src="/images/1nuevo.png"/>&nbsp;&nbsp;<span class="uiButtonText"><%= link_to "Nuevo", new_mod_fiduciasconmodificacion_path %></span></div>
                 <%# end %></div></td>
            </tr>
          </table></td>
      </tr>
</table>
<table width="80%" border="0" align="center" cellspacing="1" cellpadding="1">
  <tr>
    <td class="oce-first" width="10%">Fecha</td>
    <td class="oce-first" width="15%">Tipo Modificacion</td>
    <td class="oce-first" width="10%">Valor</td>
    <td class="oce-first" width="15%">Plazo (Mes - Dias)</td>
    <td class="oce-first" width="35%">Observaciones</td>
    <td class="oce-first" width="15%">&nbsp;</td>
  </tr>
</table>
<% @fiduciasconmodificaciones.each do |fiduciasconmodificacion| %>
<table width="80%" border="0" cellspacing="0" cellpadding="0" align="center" >
  <tr class="<%=cycle('even1', 'odd1')%>">
    <td width="10%" class="camposDesc"><%=fiduciasconmodificacion.fecha%></td>
    <td width="15%" class="camposDesc"><%=fiduciasconmodificacion.tipo_modificacion %></td>
    <td width="10%" class="camposDesc">$ <%=number_to_currency(fiduciasconmodificacion.valor,{:precision => 0, :unit=>"", :delimiter =>"."})%></td>
    <td width="15%" class="camposDesc"><div align="center"><%=fiduciasconmodificacion.plazo_mes%> - <%=fiduciasconmodificacion.plazo_dias%></div></td>
    <td width="35%" class="camposDesc"><%=fiduciasconmodificacion.observacion %></td>
    <td width="15%"><div align="center">
        <%# if permiso("fiduciasconmodificacion","A").to_s == "S" %>
       <%= link_to 'Editar', edit_mod_fiduciasconmodificacion_path(@fiduciascontrato, fiduciasconmodificacion), :class=>"editarlink"%>
       <%# end %>
        <%# if permiso("fiduciasconmodificacion","E").to_s == "S" %>&nbsp;|
       <%= link_to 'Borrar',mod_fiduciasconmodificacion_path(@fiduciascontrato, fiduciasconmodificacion),
                :confirm => 'Esta Seguro de Eliminar?', :method => :delete, :class=>"borrarlink"%>
        <%#end%>
</div></td>
  </tr>
</table>
<% end %>
<br />
