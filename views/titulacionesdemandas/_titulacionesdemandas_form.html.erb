<table width="837" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td><table width="100%" border="0" cellspacing="2" cellpadding="0">
        <tr class="odd1">
          <td width="25%" class="campos">Tipo de proceso *</td>
          <td width="30%" class="campos">Radicado *</td>
          <td width="15%" class="campos">Fecha radicado</td>
          <td width="30%" class="campos">Juzgado</td>
        </tr>
        <tr>
          <td class="campos">
              <%= select(:titulacionesdemanda,:tipoproceso, [["FISCAL","FISCAL"],["PERTENENCIA","PERTENENCIA"]],
              { :include_blank => true },{ :style=>"width:150px", :class => "campo" })%>
              <%= error_message_on :titulacionesdemanda, :tipoproceso, :css_class =>"cerror"%></td>
          <td class="campos">
              <%= text_field :titulacionesdemanda,:radicado ,{ :class => "campo", :size => 40,
                :onChange=> "javascript:this.value=this.value.toUpperCase();",
                :onFocus => "this.style.border='1px solid #f00'",
                :onBlur => "this.style.border='1px solid #bbb'" }%>
              <%= error_message_on :titulacionesdemanda, :radicado, :css_class =>"cerror"%></td>
          <td class="camposDesc">
            <%= calendar_date_select "titulacionesdemanda", "fecha_radicado", :year_range => select_rangocalendar, :class=> "campo", :size => 10, :time => false, :onfocus=>"this.blur()"%>
          </td>
          <td class="camposDesc"><%= text_field :titulacionesdemanda, :juzgado ,{ :class => "campo", :size => 40,
                :onChange=> "javascript:this.value=this.value.toUpperCase();",
                :onFocus => "this.style.border='1px solid #f00'",
                :onBlur => "this.style.border='1px solid #bbb'" }%>
          <%= error_message_on :titulacionesdemanda, :juzgado, :css_class =>"cerror"%>
        </td>
        </tr>
      </table>
      <table width="100%" border="0" cellspacing="2" cellpadding="0">
        <tr class="odd1">
          <td width="30%" class="campos">Estado *</td>
          <td width="20%" class="campos">Notificado</td>
          <td width="20%" class="campos">Inspeccion Judifical</td>
          <td width="30%" class="campos">Sentencia</td>
        </tr>
        <tr>
        <td class="campos"><%= select(:titulacionesdemanda,:estado, [["REPARTO","REPARTO"],
                                              ["ADMITIDO","ADMITIDO"],
                                              ["NO ADMITIDO","NO ADMITIDO"],
                                              ["RECHAZADO","RECHAZADO"],
                                            ["RADICADO","RADICADO"]],
                        { :include_blank => true },
                      { :style=>"width:200px", :class => "campo" })%>
          <%= error_message_on :titulacionesdemanda, :estado, :css_class =>"cerror"%>
        </td>
        <td class="campos"><%= select(:titulacionesdemanda,:notificado, [["SI","SI"],
                                              ["NO","NO"]],
                        { :include_blank => true },
                      { :style=>"width:120px", :class => "campo" })%>
          <%= error_message_on :titulacionesdemanda, :notificado, :css_class =>"cerror"%>
        </td>
        <td class="campos"><%= select(:titulacionesdemanda,:inspeccion, [["SI","SI"],
                                              ["NO","NO"]],
                        { :include_blank => true },
                      { :style=>"width:120px", :class => "campo" })%>
          <%= error_message_on :titulacionesdemanda, :inspeccion, :css_class =>"cerror"%>
        </td>
        <td class="campos"><%= select(:titulacionesdemanda,:sentencia, [["ESTIMADO (POSITIVO)","ESTIMADO (POSITIVO)"],
                                              ["DESESTIMADO (NEGATIVO)","DESESTIMADO (NEGATIVO)"]],
                        { :include_blank => true },
                      { :style=>"width:200px", :class => "campo" })%>
          <%= error_message_on :titulacionesdemanda, :sentencia, :css_class =>"cerror"%>
        </td>
        </tr>
      </table>
      <table width="100%" border="0" cellspacing="2" cellpadding="0">
        <tr class="odd1">
          <td class="campos">Demanda Elaborada</td>
          <td class="campos">Abogado Asignado</td>
          <td class="campos">Observaciones</td>
        </tr>
        <tr valign="top">
        <td class="campos"><%= select(:titulacionesdemanda,:elaborado, [["SI","SI"],
                                              ["NO","NO"]],
                        { :include_blank => true },
                      { :style=>"width:120px", :class => "campo" })%>
          <%= error_message_on :titulacionesdemanda, :elaborado, :css_class =>"cerror"%>
        </td>
        <td class="campos"><%=select("titulacionesdemanda", "user_juridico", User.find(:all,:conditions=>["activo='S'"], :order=>['nombre']).collect {|p| [ p.nombre, p.id ] }, { :include_blank => true },
                    { :style=>"width:380px", :class => "campo", :onFocus => "this.style.border='1px solid #f00'", :onBlur => "this.style.border='1px solid #bbb'" }) %>
          <%= error_message_on :titulacionesdemanda, :user_juridico, :css_class =>"cerror"%>
        </td>
         <td class="campos">
          <%=text_area(:titulacionesdemanda, :observaciones, :cols => 60, :rows => 4, :class => 'campo', :maxlength => 3900,
          :onChange=> "javascript:this.value=this.value.toUpperCase();") %>
          <%= error_message_on :titulacionesdemanda, :observaciones, :css_class =>"cerror"%></td>
        
        </tr>
      </table>
    <table width="100%" height="0" border="0" cellpadding="0" cellspacing="0">
      <tr>
        <td height="10"><hr size="1" noshade class="hrlinea"/></td>
      </tr>
    </table>
    <table width="100%" border="0" cellpadding="2" cellspacing="2">
      <tr>
        <td  height="30"><div align="right"><div class="uiButton"><img class="img" src="/images/guardar.png"/>&nbsp;&nbsp;<span class="uiButtonText"><%= hidden_field :titulacionesdemanda, :id %><%= submit_tag "Guardar", :disable_with => "Guardando..."%></span></div></div></td>
      </tr>
    </table>
    </td>
  </tr>
</table>
