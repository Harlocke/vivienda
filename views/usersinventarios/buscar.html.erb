   <%= stylesheet_link_tag 'scaffold'%>
   <%=  javascript_include_tag :defaults %>
<table width="100%" height="40">
      <tr>
        <td>
          <table width="100%" height="100%" border="0" cellpadding="0" cellspacing="0">
            <tr>
              <td width="3%"><img src="/images/usersinventariomin.png" width="64" height="64"/></td>
              <td width="94%"><div class="col_enc">INVENTARIOS POR USUARIOS</div></td>
              <td width="3%"><div align="right"><%= link_to image_tag('atras.png',
	            { :border=>0, :title => "Buscar"}),busqueda_usersinventarios_path %>
                </div></td>
            </tr>
          </table></td>
      </tr>
</table>
<br/>
<table width="90%" border="0" align="center" cellpadding="0" cellspacing="0">
  <tr valign="middle">
    <td width="10%" height="30" class="oce-first"><div align="center">Codigo</div></td>
    <td width="25%" height="30" class="oce-first"><div align="center">Inventario</div></td>
    <td width="25%" class="oce-first"><div align="center">Usuario</div></td>
    <td width="25%" class="oce-first"><div align="center">Dependencia</div></td>
    <td width="10%" class="oce-first"><div align="center">Serial</div></td>
    <td width="5%" class="oce-first">&nbsp;</td>
  </tr>
     <% i = 0 %>
<% @usersinventarios.each do |usersinventario| %>
    <% i = i + 1 %>
<tr valign="middle" class="<%=cycle('even1', 'odd1')%>">
    <td height="30" class="camposDesc"><div align="center"><%=h usersinventario.codigo%></div></td>
    <td height="30" class="camposDesc"><%=h usersinventario.inventarioselemento.descripcion rescue nil %></td>
    <td height="30" class="camposDesc"><%=h usersinventario.user.nombre rescue nil %></td>
    <td class="camposDesc"><%=h usersinventario.dependencia.descripcion rescue nil %></td>
    <td class="camposDesc"><div align="center"><%=h usersinventario.serial %></div></td>
    <td class="camposDesc"><div align="center">
    <%= link_to 'Editar', edit_usersinventario_path(usersinventario),:class=>"editarlink"%>
      </div></td>
  </tr>
<% end %>
</table>
<table width="80%" height="0" align="center" border="0" cellpadding="0" cellspacing="0">
  <tr>
    <td height="10"><hr size="1" noshade class="hrlinea"/></td>
  </tr>
</table>
<table width="80%" border="0" align="center" cellpadding="0" cellspacing="0">
  <tr valign="middle">
    <td width="100%" height="30" class="camposDescN"><div align="center">Nro Total de Registro : <%=i%></div></td>
  </tr>
</table>