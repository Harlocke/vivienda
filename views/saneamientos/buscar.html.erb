<table width="100%" height="40">
  <tr>
    <td class="camposDesc">
      <table width="100%" height="100%" border="0" cellpadding="0" cellspacing="0">
        <tr>
          <td width="3%"><img src="/images/saneamientomin.png" width="64" height="64"/></td>
          <td width="94%"><div class="col_enc">SANEAMIENTO PREDIAL</div></td>
          <td width="3%"><div align="right"><%= link_to image_tag('atras.png',{ :border=>0, :title => "Buscar"}),saneamientos_path %>
          </div></td>
        </tr>
      </table></td>
  </tr>
</table>
<div align="center">
<table width="90%" border="0" align="center" cellpadding="0" cellspacing="0">
  <tr>
    <td width="10%" height="30" class="oce-first">Nro matricula</td>
    <td width="10%" class="oce-first">CBML</td>
    <td width="10%" class="oce-first">Proyecto</td>
    <td width="20%" class="oce-first">Comuna</td>
    <td width="10%" class="oce-first">Identificacion</td>
    <td width="10%" class="oce-first">Nombre razonsocial</td>
    <td width="30%" class="oce-first">Clasificacion del proceso</td>
    <td width="10%" class="oce-first"></td>
  </tr>
<% @saneamientos.each do |saneamiento| %>
  <tr valign="middle" class="<%=cycle('even1', 'odd1')%>">
    <td class="camposDesc"><%=h saneamiento.nro_matricula %></td>
    <td class="camposDesc"><%=h saneamiento.cbml %></td>
    <td class="camposDesc"><%=h saneamiento.proyecto %></td>
    <td class="camposDesc"><%=h saneamiento.comuna.descripcion rescue nil %></td>
    <td class="camposDesc"><%=h saneamiento.identificacion %></td>
    <td class="camposDesc"><%=h saneamiento.nombre_razonsocial %></td>
    <td class="camposDesc"><%=h saneamiento.encargo_tramite %></td>
    <td class="camposDesc"><%= link_to 'Editar', edit_saneamiento_path(saneamiento), :class=>"editarlink" %></td>
  </tr>
<% end %>
</table>
<br/>
<table width="90%" height="40" border="0" cellpadding="0" cellspacing="0">
   <tr>
     <td width="100%" class="acci_titcontenido2"><div align="center">Total Registros: <%= @saneamientos.count %></div></td>
   </tr>
</table>
<br/>
</div>