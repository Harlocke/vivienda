<h1>New agendasuser</h1>

<% form_for(@agendasuser) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :agenda %><br />
    <%= f.text_field :agenda %>
  </p>
  <p>
    <%= f.label :user %><br />
    <%= f.text_field :user %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', agendasusers_path %>