<%#*<div align="center">%>
<%# form_for :entrepano, @entrepano,
            :url => estanteria_entrepano_path(@estanteria, @entrepano),
            :html => {:method => :put} do |f| %>
  <%#= f.error_messages %>
  <%#= render :partial => 'form', :locals => { :f => f } %>
  <%#*<table width="80%" height="0" border="0" cellpadding="0" cellspacing="0">%>
      <%#*<tr>%>
        <%#*<td height="10"><hr size="1" noshade class="hrlinea"/></td>%>
      <%#*</tr>%>
    <%#*</table>%>
<%#*<table width="80%" border="0" cellpadding="2" cellspacing="2">%>
  <%#*<tr>%>
    <%#*<td  height="30"><div align="right">%>
        <%# if permiso("entrepanos","A").to_s == "S" %>
        <%#= submit_tag "Guardar", :disable_with => "Guardando...", :class=>"buttonAsLink"%>
        <%# end %>
        <%#*</div>%>
    <%#*</td>%>
  <%#*</tr>%>
<%#*</table>%>
<%# end %>
  <%#*</div>%>