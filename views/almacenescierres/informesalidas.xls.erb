#encoding: utf-8
<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">

<ss:Styles>
  <ss:Style ss:ID='header'><ss:Font ss:Bold='1' ss:Color='#F5F6CE'/><ss:Interior ss:Color="#045FB4" ss:Pattern="Solid"/><ss:Alignment ss:Horizontal='Center'/></ss:Style>
  <ss:Style ss:ID='row'><ss:Alignment ss:Horizontal='Left' ss:Vertical='Center'/></ss:Style> 
</ss:Styles>

  <ss:Worksheet ss:Name="Salidas">
    <ss:Table>
        <ss:Column ss:Width="200"/>
        <ss:Column ss:Width="100"/>
        <ss:Column ss:Width="350"/>
        <ss:Column ss:Width="100"/>
        <ss:Column ss:Width="100"/>
        <ss:Column ss:Width="300"/>
      <ss:Row ss:StyleID='header'>
        <Cell><Data ss:Type='String'>Usuario</Data></Cell>
        <Cell><Data ss:Type='String'>Codigo</Data></Cell>
        <Cell><Data ss:Type='String'>Descripcion</Data></Cell>
        <Cell><Data ss:Type='String'>Fecha Entrega</Data></Cell>
        <Cell><Data ss:Type='String'>Cantidad</Data></Cell>
        <Cell><Data ss:Type='String'>Observaciones</Data></Cell>
      </ss:Row>
      <% @almacenessalidas.each do |almacenessalida| %>
      <ss:Row ss:StyleID='row'>
        <Cell><Data ss:Type="String"><%=almacenessalida.user.nombre rescue nil%></Data></Cell>
        <Cell><Data ss:Type="String"><%=almacenessalida.producto.codigo rescue nil%></Data></Cell>
        <Cell><Data ss:Type="String"><%=almacenessalida.producto.descripcion rescue nil%></Data></Cell>
        <Cell><Data ss:Type="String"><%=almacenessalida.created_at rescue nil%></Data></Cell>
        <Cell><Data ss:Type="String"><%=almacenessalida.cantidad rescue nil%></Data></Cell>
        <Cell><Data ss:Type="String"><%=almacenessalida.observaciones rescue nil%></Data></Cell>
      </ss:Row>
      <% end %>
    </ss:Table>
  </ss:Worksheet>
</Workbook>
