<fieldset>
<table width="100%" border="0" cellpadding="2" cellspacing="2">
  <tr>
    <td height="30"><div class="col_enc">INFORMACION FERIA 2010</div></td>
  </tr>
</table>
<% @persona.feria2010jefes.each do |feria2010jefe| %>
    <% if feria2010jefe.id != @feria2010jefe.id %>
   <tr class="<%=cycle('even1', 'odd1')%>" id="feria2010jefe_<%=feria2010jefe.id%>">
<div align="center">
<table width="837" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td><table width="100%" border="0" cellspacing="2" cellpadding="0">
        <tr class="odd1">
          <td width="17%" class="campos">Nro Formulario</td>
          <td width="13%" class="campos">Documento</td>
          <td width="15%" class="campos">Identificacion</td>
          <td width="36%" class="campos">Nombre</td>
          <td width="19%" class="campos">Fecha Nacimiento </td>
        </tr>
        <tr>
          <td class="camposDesc"><%=feria2010jefe.nroformulario %></td>
          <td class="camposDesc"><%=feria2010jefe.documento %></td>
          <td class="camposDesc"><%=feria2010jefe.identificacion %></td>
          <td class="camposDesc"><%=feria2010jefe.primernombre %>&nbsp;<%=feria2010jefe.segundonombre %>&nbsp;<%=feria2010jefe.primerapellido %>&nbsp;<%=feria2010jefe.segundoapellido %></td>
          <td class="camposDesc"><%=feria2010jefe.fecha_nacimiento %></td>
        </tr>
      </table>
	  <table width="100%" border="0" cellspacing="2" cellpadding="0">
        <tr class="odd1">
          <td width="20%" class="campos">Parentesco</td>
          <td width="23%" class="campos">Estado Civil </td>
          <td width="15%" class="campos">Ocupacion</td>
          <td width="24%" class="campos">Condici&oacute;n Especial </td>
          <td width="17%" class="campos">Ingresos Mensuales</td>
        </tr>
        <tr>
          <td class="camposDesc"><%=feria2010jefe.parentesco %></td>
          <td class="camposDesc"><%=feria2010jefe.estadocivil %></td>
          <td class="camposDesc"><%=feria2010jefe.ocupacion %></td>
          <td class="camposDesc"><%=feria2010jefe.condicionespecial %></td>
          <td class="camposDesc"><%=feria2010jefe.ingresos_mensuales %></td>
        </tr>
      </table>
	  <table width="100%" border="0" cellspacing="2" cellpadding="0">
        <tr class="odd1">
          <td width="26%" class="campos">Direcci&oacute;n</td>
          <td width="21%" class="campos">Barrio</td>
          <td width="21%" class="campos">Ciudad</td>
          <td width="15%" class="campos">Telefono 1 </td>
          <td width="15%" class="campos">Telefono 2 </td>
        </tr>
        <tr>
          <td class="camposDesc"><%=feria2010jefe.direccion %></td>
          <td class="camposDesc"><%=feria2010jefe.barrio %></td>
          <td class="camposDesc"><%=feria2010jefe.ciudad %></td>
          <td class="camposDesc"><%=feria2010jefe.telefonos1 %></td>
          <td class="camposDesc"><%=feria2010jefe.telefonos2 %></td>
        </tr>
      </table>
  <table width="100%" border="0" cellspacing="2" cellpadding="0">
        <tr class="odd1">
          <td width="16%" class="campos">Celular</td>
          <td width="15%" class="campos">Antiguedad A&ntilde;os </td>
          <td width="17%" class="campos">Antiguedad Meses </td>

          <td width="7%" class="campos">JAL</td>
          <td width="6%" class="campos">Ahorro</td>
          <td width="11%" class="campos">Valor Ahorro </td>
          <td width="6%" class="campos">Sisben</td>
          <td width="6%" class="campos">OPV</td>
          <td width="16%" class="campos">Cual OPV </td>
        </tr>
        <tr>
          <td class="camposDesc"><%=h feria2010jefe.celular %></td>
          <td class="camposDesc"><%=h feria2010jefe.antiguedad_annos %></td>
          <td class="camposDesc"><%=h feria2010jefe.antiguedad_meses %></td>
          <td class="camposDesc"><%=h feria2010jefe.jal %></td>
          <td class="camposDesc"><%=h feria2010jefe.ahorro %></td>
          <td class="camposDesc"><%=h feria2010jefe.valor_ahorro %></td>
          <td class="camposDesc"><%=h feria2010jefe.sisben %></td>
          <td class="camposDesc"><%=h feria2010jefe.opv %></td>
          <td class="camposDesc"><%=h feria2010jefe.opv_cual %></td>
        </tr>
      </table>
	  <table width="100%" border="0" cellspacing="2" cellpadding="0">
        <tr class="odd1">
          <td width="18%" class="campos">Fecha y Hora Registro </td>
          <td width="27%" class="campos">Codigo Barrio </td>
          <td width="18%" class="campos">Site Direccion </td>
          <td width="18%" class="campos">Site Barrio </td>
          <td width="19%" class="campos">Site Estado </td>
        </tr>
        <tr>
          <td class="camposDesc"><%=h feria2010jefe.fechahora_reg %></td>
          <td class="camposDesc"><%=h feria2010jefe.codigobarrio %></td>
          <td class="camposDesc"><%=h feria2010jefe.site_direccion_ubicacion %></td>
          <td class="camposDesc"><%=h feria2010jefe.site_barrio %></td>
          <td class="camposDesc"><%=h feria2010jefe.site_estado %></td>
        </tr>
      </table></td>
  </tr>
</table>
</div>
<br/>
<%  if feria2010jefe.feria2010beneficiarios.exists? %>
<table width="100%" border="0" cellpadding="2" cellspacing="2">
  <tr>
    <td height="30"><div class="col_enc">BENEFICIARIOS</div></td>
  </tr>
</table>
<div align="center">
  <% varconta = 0 %>
<% feria2010jefe.feria2010beneficiarios.each do |feria2010beneficiario| %>
  <%   varconta = varconta + 1;   %>
<table width="837" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td width="30" class="campos"><div align="center"><%= varconta %></div></td>
    <td><table width="100%" border="0" cellspacing="2" cellpadding="0">
        <tr class="odd1">
          <td width="11%" class="campos">Documento</td>
          <td width="13%" class="campos">Identificacion</td>
          <td width="39%" class="campos">Nombre</td>
          <td width="16%" class="campos">Fecha Nacimiento </td>
          <td width="21%" class="campos">Parentesco</td>
        </tr>
        <tr>
          <td class="camposDesc"><%=h feria2010beneficiario.documento %></td>
          <td class="camposDesc"><%=h feria2010beneficiario.identificacion %></td>
          <td class="camposDesc"><%=h feria2010beneficiario.primernombre %>&nbsp;<%=h feria2010beneficiario.segundonombre %>&nbsp; <%=h feria2010beneficiario.primerapellido %>&nbsp;<%=h feria2010beneficiario.segundoapellido %></td>
          <td class="camposDesc"><%=h feria2010beneficiario.fecha_nacimiento %></td>
          <td class="camposDesc"><%=h feria2010beneficiario.parentesco %></td>
        </tr>
    </table>
      <table width="100%" border="0" cellspacing="2" cellpadding="0">
        <tr class="odd1">
          <td width="25%" class="campos">Estado Civil </td>
          <td width="23%" class="campos">Ocupacion</td>
          <td width="31%" class="campos">Condici&oacute;n Especial </td>
          <td width="21%" class="campos">Ingresos Mensuales</td>
        </tr>
        <tr>
          <td class="camposDesc"><%=h feria2010beneficiario.estadocivil %></td>
          <td class="camposDesc"><%=h feria2010beneficiario.ocupacion %></td>
          <td class="camposDesc"><%=h feria2010beneficiario.condicionespecial %></td>
          <td class="camposDesc"><%=h feria2010beneficiario.ingresos_mensuales %></td>
        </tr>
      </table>
        <table width="100%" height="0" border="0" cellpadding="0" cellspacing="0">
      <tr>
        <td height="10"><hr size="1" noshade class="hrlinea"/></td>
      </tr>
    </table></td>
  </tr>
</table>
<% end %>
</div>
<br/>
<% end %>
<%  if feria2010jefe.feria2010rechazados.exists? %>
<table width="100%" border="0" cellpadding="2" cellspacing="2">
  <tr>
    <td height="30"><div class="col_enc">CAUSALES DE RECHAZO</div></td>
  </tr>
</table>
<div align="center">
<table width="750" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td><table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tr>
          <td class="oce-first">Variable</td>
        </tr>
        <% feria2010jefe.feria2010rechazados.each do |feria2010rechazado| %>
        <tr  class="<%=cycle('even1', 'odd1')%>">
          <td class="camposDesc">* <%=h feria2010rechazado.causarechazo %></td>
        </tr>
<% end %>
    </table></td>
  </tr>
</table>
</div>
<br/>
<% end %>
<%  if feria2010jefe.feria2010calificaciones.exists? %>
<table width="100%" border="0" cellpadding="2" cellspacing="2">
  <tr>
    <td height="30"><div class="col_enc">CALIFICACION</div></td>
  </tr>
</table>
<div align="center">
<table width="500" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td><table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tr>
          <td class="oce-first">Variable</td>
          <td class="oce-first"><div align="center">Puntaje</div></td>
        </tr>
 <% feria2010jefe.feria2010calificaciones.each do |feria2010calificacion| %>
        <tr  class="<%=cycle('even1', 'odd1')%>">
          <td width="88%" class="camposDesc"><%=h feria2010calificacion.calificacion %></td>
          <td width="12%" class="camposDesc"><div align="center"><%=h feria2010calificacion.puntaje %></div></td>
        </tr>
<% end %>
    </table></td>
  </tr>
</table>
</div>
<br/>
<% end %>
<%  if feria2010jefe.feria2010calificacionrecursos.exists? %>
<table width="100%" border="0" cellpadding="2" cellspacing="2">
  <tr>
    <td height="30"><div class="col_enc">CALIFICACION DE RECURSOS</div></td>
  </tr>
</table>
<div align="center">
<table width="500" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td><table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tr>
          <td class="oce-first">Variable</td>
          <td class="oce-first"><div align="center">Puntaje</div></td>
        </tr>
<% feria2010jefe.feria2010calificacionrecursos.each do |feria2010calificacionrecurso| %>
        <tr  class="<%=cycle('even1', 'odd1')%>">
          <td width="88%" class="camposDesc"><%=h feria2010calificacionrecurso.calificacion %></td>
          <td width="12%" class="camposDesc"><div align="center"><%=h feria2010calificacionrecurso.puntaje %></div></td>
        </tr>
<% end %>
    </table></td>
  </tr>
</table>
</div>
<br/>
<% end %>
<%  if feria2010jefe.feria2010observaciones.exists? %>
<table width="100%" border="0" cellpadding="2" cellspacing="2">
  <tr>
    <td height="30"><div class="col_enc">OBSERVACIONES</div></td>
  </tr>
</table>
<div align="center">
<table width="500" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td><table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tr>
          <td class="oce-first">Variable</td>
        </tr>
 <% feria2010jefe.feria2010observaciones.each do |feria2010observacion| %>
        <tr  class="<%=cycle('even1', 'odd1')%>">
          <td class="camposDesc"><%=h feria2010observacion.observacion %></td>
        </tr>
<% end %>
    </table></td>
  </tr>
</table>
</div>
<br/>
<% end %>
<% end %>
<% end %>
</fieldset>