<table width="100%" height="40">
      <tr>
        <td>
          <table width="100%" height="100%" border="0" cellpadding="0" cellspacing="0">
            <tr>
              <td width="3%"><img src="/images/calidadmin.png" width="64" height="64"/></td>
              <td width="94%"><div class="col_enc">CALIDAD - PROCESO( <%=@calidaddocumento.calidadproceso.descripcion rescue nil%> )</div></td>
              <td width="3%"><div align="right"><%= link_to image_tag('atras.png',
	            { :border=>0, :title => "Regresar al proceso"}),edit_calidadproceso_path(@calidaddocumento.calidadproceso_id) %>
                </div></td>
            </tr>
          </table></td>
      </tr>
</table>
<br/>
<table width="90%" border="0" align="center" cellpadding="0" cellspacing="0">
  <tr valign="middle">
    <td width="30%" height="30" class="oce-first" align="left">Codigo</td>
    <td width="30%" height="30" class="oce-first" align="left">Tipo de Documento</td>
    <td width="30%" height="30" class="oce-first" align="left">Nombre</td>
    <td width="30%" height="30" class="oce-first" align="left">Version</td>
  </tr>
   <tr>
     <td height="30" class="camposDesc"><%=@calidaddocumento.codigo rescue nil%></td>
     <td height="30" class="camposDesc"><%=@calidaddocumento.tipodocumento%> </td>
     <td height="30" class="camposDesc"><%=@calidaddocumento.nombre%> </td>
     <td height="30" class="camposDesc"><%=@calidaddocumento.version%> </td>
 </tr>
</table>
<BR/>
<table width="90%" height="40" align="center" >
  <tr>
    <td>
      <table width="100%" height="100%" border="0" cellpadding="0" cellspacing="0">
        <tr>
          <td width="94%"><div class="col_enc">&nbsp;&nbsp;DOCUMENTOS DIGITALIZADOS</div></td>
          <td width="3%"><div align="right"><div class="uiButton"><img class="img" src="/images/1nuevo.png"/>&nbsp;&nbsp;<span class="uiButtonText"><%= link_to "Nuevo", new_ima_calidaddocumentosimagen_path %></span></div></div></td>
        </tr>
      </table></td>
  </tr>
</table>
<table width="90%" border="0" align="center" cellspacing="1" cellpadding="1">
  <tr>
    <td class="oce-first" height="30" width="10%">Fecha</td>
    <td class="oce-first" width="35%">Descripcion</td>
    <td class="oce-first" width="40%">Nombre Archivo</td>
    <td class="oce-first" width="15%">&nbsp;</td>
  </tr>
<% @calidaddocumentosimagenes.each do |calidaddocumentosimagen| %>
  <tr class="<%=cycle('even1', 'odd1')%>">
    <td width="15%" class="camposDesc">
      <table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tr>
           <td class="camposDesc" align="left"><%=calidaddocumentosimagen.created_at.strftime("%Y-%m-%d %X")%></td>
        </tr>
        <tr>
           <td class="camposDesc" align="left"><a title="<%=calidaddocumentosimagen.user.nombre rescue nil%>"><%=calidaddocumentosimagen.user.username rescue nil%></a></td>
        </tr>
      </table></td>
    <td width="35%" class="camposDesc"><%=calidaddocumentosimagen.descripcion %></td>
    <td width="40%" class="camposDesc"><%=calidaddocumentosimagen.calidaddocumento_file_name %></td>
    <td width="10%" class="camposDesc"><div align="center">
         <%= link_to "Ver", calidaddocumentosimagen.calidaddocumento.url(:original, false),
                     { :popup => ['new_window','height=450,width=600,scrollbars=yes'], :class=>"verlink"}%>
        <% if permiso("calidaddocumentosimagen","E").to_s == "S" %>&nbsp;|
       <%= link_to 'Borrar', ima_calidaddocumentosimagen_path(@calidaddocumento, calidaddocumentosimagen),
                :confirm => 'Esta Seguro de Eliminar?', :method => :delete, :class=>"borrarlink"%>
       <%end%>
</div></td>
  </tr>
<% end %>
</table>
<br/>