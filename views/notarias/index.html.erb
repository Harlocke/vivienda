<h1>Listing notarias</h1>

<table>
  <tr>
    <th>Codigo</th>
    <th>Descripcion</th>
    <th>Email</th>
    <th>Zona</th>
    <th>Utilizada</th>
  </tr>

<% @notarias.each do |notaria| %>
  <tr>
    <td><%=h notaria.codigo %></td>
    <td><%=h notaria.descripcion %></td>
    <td><%=h notaria.email %></td>
    <td><%=h notaria.zona %></td>
    <td><%=h notaria.utilizada %></td>
    <td><%= link_to 'Show', notaria %></td>
    <td><%= link_to 'Edit', edit_notaria_path(notaria) %></td>
    <td><%= link_to 'Destroy', notaria, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New notaria', new_notaria_path %>