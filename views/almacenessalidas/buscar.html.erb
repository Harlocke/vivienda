<%=  javascript_include_tag :defaults %>
<table width="100%" height="40">
      <tr>
        <td>
          <table width="100%" height="100%" border="0" cellpadding="0" cellspacing="0">
            <tr>
              <td width="3%"><img src="/images/salidasmin.png" width="64" height="64"/></td>
              <td width="94%"><div class="col_enc">SALIDA DE PRODUCTOS</div></td>
              <td width="3%"><div align="right"><%= link_to image_tag('atras.png',
	            { :border=>0, :title => "Buscar"}),busqueda_almacenessalidas_path %>
                </div></td>
            </tr>
          </table></td>
      </tr>
</table>
<br/>
<table width="80%" border="0" align="center" cellpadding="0" cellspacing="0">
  <tr valign="middle">
    <td width="15%" height="30" class="oce-first"><div align="center">Fecha Entrega</div></td>
    <td width="35%" height="30" class="oce-first"><div align="center">Producto</div></td>
    <td width="25%" class="oce-first"><div align="center">Usuario</div></td>
    <td width="8%" class="oce-first"><div align="center">Cantidad</div></td>
    <td width="15%" class="oce-first">&nbsp;</td>
  </tr>
</table>
<table width="80%" border="0" align="center" cellpadding="0" cellspacing="0" id="one-column-emphasis">
<% i = 0 %>
<% @almacenessalidas.each do |almacenessalida| %>
<% i = i + 1 %>
  <tr valign="middle">
    <td width="15%" height="30" class="camposDesc"><div align="center"><%=h almacenessalida.created_at.strftime("%Y-%m-%d %X") %></div></td>
    <td width="35%" height="30" class="camposDesc"><%=h almacenessalida.producto.descripcion rescue nil %></td>
    <td width="25%" height="30" class="camposDesc"><div align="center"><%=h almacenessalida.user.nombre rescue nil %></div></td>
    <td width="8%" class="camposDesc"><div align="center"><%=h almacenessalida.cantidad %></div></td>
    <td width="15%" class="camposDesc"><div align="center">
    <%= link_to 'Editar', edit_almacenessalida_path(almacenessalida),:class=>"editarlink"%>
    <% if permiso("almacenessalida","E").to_s == "S" %>
      &nbsp;|&nbsp;<%= link_to 'Borrar', almacenessalida_path(almacenessalida), :confirm => 'Esta Seguro de Eliminar?',:method => :delete, :class=>"borrarlink" %>
    <% end %>
      </div></td>
  </tr>
<% end %>
</table>
<table width="80%" height="0" align="center" border="0" cellpadding="0" cellspacing="0">
  <tr>
    <td height="10"><hr size="1" noshade class="hrlinea"/></td>
  </tr>
</table>
<table width="80%" border="0" align="center" cellpadding="0" cellspacing="0">
  <tr valign="middle">
    <td width="100%" height="30" class="camposDescN"><div align="center">Nro Total de Registro : <%=i%></div></td>
  </tr>
</table>