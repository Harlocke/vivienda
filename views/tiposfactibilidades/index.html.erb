<h1>Listing tiposfactibilidades</h1>

<table>
  <tr>
    <th>Fase</th>
    <th>Descripcion fase</th>
    <th>Codigo</th>
    <th>Actividad</th>
  </tr>

<% @tiposfactibilidades.each do |tiposfactibilidad| %>
  <tr>
    <td><%=h tiposfactibilidad.fase %></td>
    <td><%=h tiposfactibilidad.descripcion_fase %></td>
    <td><%=h tiposfactibilidad.codigo %></td>
    <td><%=h tiposfactibilidad.actividad %></td>
    <td><%= link_to 'Show', tiposfactibilidad %></td>
    <td><%= link_to 'Edit', edit_tiposfactibilidad_path(tiposfactibilidad) %></td>
    <td><%= link_to 'Destroy', tiposfactibilidad, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New tiposfactibilidad', new_tiposfactibilidad_path %>