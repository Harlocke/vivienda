      <tr valign="middle">
        <td width="93%" height="20" class="oce-first">Descripcion</td>
        <td width="7%" class="oce-first">&nbsp;</td>
      </tr>
    <% @mensajesenviosc.each do |mensajesenvio| %>
      <tr valign="middle" class="<%=cycle('even1', 'odd1')%>" >
        <td height="20" class="camposDesc"><div align="justify"><strong><%=h mensajesenvio.created_at.strftime("%Y-%m-%d %X") %> </strong> - <%=h mensajesenvio.descripcion%></div></td>
        <td class="camposDesc"><div align="center">
          <% if mensajesenvio.estado == "V"%>
            <%= image_tag("connect.png",:size => "16x16")%>
          <% else %>
              <%= link_to_remote image_tag('connect2.png', { :border => 0, :title => "Verificado"}),
              :url => mensajesenvio_url(:id => mensajesenvio), :method => :delete,
              :before => "$(this).up('.post1S_#{mensajesenvio.id}').remove()"%>
          <% end %>
          </div></td>
      </tr>
    <% end %>
    </table>
  </div>
