<table width="100%" height="40">
      <tr>
        <td>
          <table width="100%" height="100%" border="0" cellpadding="0" cellspacing="0">
            <tr>
              <td width="3%"><img src="/images/fiduciamin.png" width="64" height="64"/></td>
              <td width="94%"><div class="col_enc">&nbsp;&nbsp;INFORMACION EXTRACTO</div></td>
              <td width="3%"><div align="right"><div class="uiButton"><img class="img" src="/images/1atras.png"/>&nbsp;&nbsp;<span class="uiButtonText"><%= link_to 'Regresar',  edit_fiducia_path(@fiduciascontrato.fiducia_id) %></span></div></div>            </td>
            </tr>
          </table></td>
      </tr>
</table>
<table width="80%" border="0" align="center" cellpadding="0" cellspacing="0">
  <tr valign="middle">
    <td width="10%" height="30" class="oce-first"><div align="center">Nro Fondo</div></td>
    <td width="50%" height="30" class="oce-first">Nombre del Fondo</td>
    <td width="40%" height="30" class="oce-first">Proyecto</td>
  </tr>
   <tr>
     <td height="10" class="camposDesc"><div align="center"><%=@fiduciascontrato.nro_fondo rescue nil%></div></td>
     <td height="50" class="camposDesc"><%=@fiduciascontrato.fondo rescue nil%></td>
     <td height="40" class="camposDesc"><%=@fiduciascontrato.proyecto rescue nil%></td>
 </tr>
</table>
   <br/>
<table width="80%" height="40" align="center" >
      <tr>
        <td>
          <table width="100%" height="100%" border="0" cellpadding="0" cellspacing="0">
            <tr>
              <td width="94%"><div class="col_enc">&nbsp;&nbsp;EXTRACTOS</div></td>
              <td width="3%"><div align="right">
                  <%# if permiso("fiduciasimagen","C").to_s == "S" %>
                   <div class="uiButton"><img class="img" src="/images/1nuevo.png"/>&nbsp;&nbsp;<span class="uiButtonText"><%= link_to "Nuevo", new_img_fiduciasimagen_path %></span></div>
                 <%# end %></div></td>
            </tr>
          </table></td>
      </tr>
</table>
<table width="80%" border="0" align="center" cellspacing="1" cellpadding="1">
  <tr>
    <td class="oce-first" width="10%">Fecha</td>
    <td class="oce-first" width="35%">Descripcion</td>
    <td class="oce-first" width="40%">Nombre Archivo</td>
    <td class="oce-first" width="15%">&nbsp;</td>
  </tr>
</table>
<% @fiduciasimagenes.each do |fiduciasimagen| %>
<table width="80%" border="0" cellspacing="0" cellpadding="0" align="center" >
  <tr class="<%=cycle('even1', 'odd1')%>">
    <td width="10%" class="camposDesc"><%=fiduciasimagen.created_at%></td>
    <td width="35%" class="camposDesc"><%#=fiduciasimagen.descripcion %></td>
    <td width="40%" class="camposDesc"><%=fiduciasimagen.doc_file_name %></td>
    <td width="15%" class="camposDesc"><div align="center">
         <%= link_to "Ver", fiduciasimagen.doc.url(:original, false),
             { :popup => ['new_window','height=450,width=600,scrollbars=yes'], :class=>"verlink"}%>
        <%# if permiso("fiduciasimagen","E").to_s == "S" %>&nbsp;|
       <%= link_to 'Borrar', img_fiduciasimagen_path(@fiduciascontrato, fiduciasimagen),
                :confirm => 'Esta Seguro de Eliminar?', :method => :delete, :class=>"borrarlink"%>
       <%#end%>
</div></td>
  </tr>
</table>
<% end %>
<br />
