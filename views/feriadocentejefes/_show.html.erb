          <td class="camposDesc"><%=h feriadocentebeneficiario.condicionespecial %></td>
          <td class="camposDesc"><%=h feriadocentebeneficiario.ingresos_mensuales %></td>
        </tr>
      </table>
        <table width="100%" height="0" border="0" cellpadding="0" cellspacing="0">
      <tr>
        <td height="10"><hr size="1" noshade class="hrlinea"/></td>
      </tr>
    </table></td>
  </tr>
</table>
<% end %>
</div>
<br/>
<% end %>
<%  if feriadocentejefe.feriadocenterechazados.exists? %>
<table width="100%" border="0" cellpadding="2" cellspacing="2">
  <tr>
    <td height="30"><div class="col_enc">CAUSALES DE RECHAZO</div></td>
  </tr>
</table>
<div align="center">
<table width="750" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td><table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tr>
          <td class="oce-first">Variable</td>
        </tr>
        <% feriadocentejefe.feriadocenterechazados.each do |feriadocenterechazado| %>
        <tr>
          <td class="camposDesc">* <%=h feriadocenterechazado.causarechazo %></td>
        </tr>
<% end %>
    </table></td>
  </tr>
</table>
</div>
<br/>
<% end %>
<%  if feriadocentejefe.feriadocentecalificaciones.exists? %>
<table width="100%" border="0" cellpadding="2" cellspacing="2">
  <tr>
    <td height="30"><div class="col_enc">CALIFICACION</div></td>
  </tr>
</table>
<div align="center">
<table width="500" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td><table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tr>
          <td class="oce-first">Variable</td>
          <td class="oce-first"><div align="center">Puntaje</div></td>
        </tr>
 <% feriadocentejefe.feriadocentecalificaciones.each do |feriadocentecalificacion| %>
        <tr>
          <td width="88%" class="camposDesc"><%=h feriadocentecalificacion.calificacion %></td>
          <td width="12%" class="camposDesc"><div align="center"><%=h feriadocentecalificacion.puntaje %></div></td>
        </tr>
<% end %>
    </table></td>
  </tr>
</table>
</div>
<br/>
<% end %>
<% end %>
<% end %>