          <td class="camposDesc"><%=h feriadocentejefe.antiguedad_annos %></td>
          <td class="camposDesc"><%=h feriadocentejefe.antiguedad_meses %></td>
          <td class="camposDesc"><%=h feriadocentejefe.jal %></td>
          <td class="camposDesc"><%=h feriadocentejefe.valor_ahorro %></td>
          <td class="camposDesc"><%=h feriadocentejefe.sisben %></td>
          <td class="camposDesc"><%=h feriadocentejefe.opv %></td>
          <td class="camposDesc"><%=h feriadocentejefe.opv_cual %></td>
        </tr>
      </table>
</td>
  </tr>
</table>
</div>
<br/>
<%  if feriadocentejefe.feriadocentebeneficiarios.exists? %>
<table width="100%" border="0" cellpadding="2" cellspacing="2">
  <tr>
    <td height="30"><div class="col_enc">BENEFICIARIOS</div></td>
  </tr>
</table>
<div align="center">
  <% varconta = 0 %>
