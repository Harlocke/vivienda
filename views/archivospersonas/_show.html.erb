<fieldset>
<table width="100%" border="0" cellpadding="2" cellspacing="2">
  <tr>
    <td height="30"><div class="col_enc">INFORMACION REASENTAMIENTO</div></td>
  </tr>
</table>
  <br/>
<% @persona.iguanaspersonas.each do |iguanaspersona| %>
    <% if iguanaspersona.id != @iguanaspersona.id %>
  <div align="center">
<table width="70%" align="center" id="one-column-emphasis">
  <tr>
    <td width="30%" class="campos" align="left"><div align="left">Proyecto</div></td>
    <td width="70%" align="left" class="camposDesc"><div align="left"><%=iguanaspersona.iguana.proyecto rescue nil%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">Identificacion Plano</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=iguanaspersona.iguana.nro_registro rescue nil%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">Social Asignado</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=User.find(iguanaspersona.iguana.social_asignado).nombre rescue nil%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">Juridico Asignado</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=User.find(iguanaspersona.iguana.juridico_asignado).nombre rescue nil%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">Sector</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=iguanaspersona.iguana.sector rescue nil%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">Encuesta</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=iguanaspersona.iguana.encuesta rescue nil%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">Cobama</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=iguanaspersona.iguana.cobama rescue nil%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">Matricula</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=iguanaspersona.iguana.matricula rescue nil%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">Direccion</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=iguanaspersona.iguana.direccion rescue nil%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">Opcion de Vivienda</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=iguanaspersona.iguana.opcion_vivienda rescue nil%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">Estado General</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=iguanaspersona.iguana.iguanasestado.descripcion rescue nil%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left">&nbsp;</td>
    <td align="left" class="camposDesc">
       <% if permiso("iguana","C").to_s == "S" %>
          <div align="right">
            <div class="uiButton"><img class="img" src="/images/ver3.png"/>&nbsp;&nbsp;<span class="uiButtonText"><%= link_to 'Ver Informacion', edit_iguana_path(iguanaspersona.iguana_id) %></span></div>
          </div>
       <% end %>
    </td>
  </tr>
</table>
<table width="70%" height="0" border="0" cellpadding="0" cellspacing="0">
<tr>
  <td height="10"><hr size="1" noshade class="hrlinea"/></td>
</tr>
</table>
    <br/>
     <% end %>
  </div>
<% end %>
</fieldset>
