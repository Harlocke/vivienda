<h1>Editing clasificacion</h1>

<% form_for(@clasificacion) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :descripcion %><br />
    <%= f.text_field :descripcion %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @clasificacion %> |
<%= link_to 'Back', clasificaciones_path %>