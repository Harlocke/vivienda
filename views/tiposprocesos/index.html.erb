<h1>Listing tiposprocesos</h1>

<table>
  <tr>
    <th>Descripcion</th>
  </tr>

<% @tiposprocesos.each do |tiposproceso| %>
  <tr>
    <td><%=h tiposproceso.descripcion %></td>
    <td><%= link_to 'Show', tiposproceso %></td>
    <td><%= link_to 'Edit', edit_tiposproceso_path(tiposproceso) %></td>
    <td><%= link_to 'Destroy', tiposproceso, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New tiposproceso', new_tiposproceso_path %>