<table width="100%" border="0" cellpadding="2" cellspacing="2">
  <tr>
    <td height="30"><div class="col_enc">OBSERVACIONES ANTERIORES</div></td>
  </tr>
</table>
<table width="100%" border="0" cellpadding="0" cellspacing="0">
  <tr valign="middle">
    <td width="15%" height="20" class="oce-first">Elimina</td>
    <td width="15%" height="20" class="oce-first">Fecha</td>
    <td width="15%" class="oce-first">Atencion</td>
    <td width="55%" class="oce-first">Observacion</td>
  </tr>
<% @vivienda.antobservaviviendas.each do |antobservavivienda| %>
   <tr  valign="middle" id="antobservavivienda_<%=antobservavivienda.id%>">
    <td class="camposDesc">
       <table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tr>
           <td class="camposDesc" align="left"><%=antobservavivienda.fecha_desvincula.strftime("%Y-%m-%d %X") rescue nil%></td>
        </tr>
        <tr>
           <td class="camposDesc" align="left"><a title="<%=User.find(antobservavivienda.user_desvincula).nombre rescue nil%>"><%=User.find(antobservavivienda.user_desvincula).username rescue nil%></a></td>
        </tr>
      </table>
    </td>
    <td align="left" width="15%" >
      <table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tr>
           <td class="camposDesc" align="left"><%=antobservavivienda.created_at.strftime("%Y-%m-%d %X")%></td>
        </tr>
        <tr>
           <td class="camposDesc" align="left"><a title="<%=antobservavivienda.user.nombre rescue nil%>"><%=antobservavivienda.user.username rescue nil%></a></td>
        </tr>
      </table>
    </td>
    <td class="camposDesc" align="left"><%=antobservavivienda.dtipoatencion rescue nil%></td>
    <td class="camposDesc" align="left"><%=antobservavivienda.observacion%></td>
 </tr>
<% end %>
</table>
