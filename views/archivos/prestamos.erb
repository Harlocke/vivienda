<strong>INFORMACION PRESTAMOS</strong>
<br/>
<table width="100%" border="1" >
  <tr bgcolor="#00CCFF" valign="middle">
    <td width="50%" style="font-size: 11px; font-family: Tahoma;"><strong>Persona</strong></td>
    <td width="50%" style="font-size: 11px; font-family: Tahoma;"><strong>Contratista</strong></td>
    <td width="50%" style="font-size: 11px; font-family: Tahoma;"><strong>Caja</strong></td>
    <td width="50%" style="font-size: 11px; font-family: Tahoma;"><strong>Carpeta</strong></td>
    <td width="50%" style="font-size: 11px; font-family: Tahoma;"><strong>Manzana</strong></td>
    <td width="50%" style="font-size: 11px; font-family: Tahoma;"><strong>Lote</strong></td>
    <td width="20%" style="font-size: 11px; font-family: Tahoma;"><strong>Consecutivo Inicil</strong></td>
    <td width="20%" style="font-size: 11px; font-family: Tahoma;"><strong>Consecutivo Final</strong></td>
    <td width="20%" style="font-size: 11px; font-family: Tahoma;"><strong>Resolucion Inicial</strong></td>
    <td width="20%" style="font-size: 11px; font-family: Tahoma;"><strong>Resolucion Final</strong></td>
    <td width="50%" style="font-size: 11px; font-family: Tahoma;"><strong>Nro Folios Prestados</strong></td>
    <td width="50%" style="font-size: 11px; font-family: Tahoma;"><strong>Fecha Prestamo</strong></td>
    <td width="50%" style="font-size: 11px; font-family: Tahoma;"><strong>Usuario realiza el Prestamo</strong></td>
    <td width="50%" style="font-size: 11px; font-family: Tahoma;"><strong>Prestamo a</strong></td>
    <td width="50%" style="font-size: 11px; font-family: Tahoma;"><strong>Fecha Vencimiento</strong></td>
    <td width="50%" style="font-size: 11px; font-family: Tahoma;"><strong>Fecha Devolucion</strong></td>
    <td width="50%" style="font-size: 11px; font-family: Tahoma;"><strong>Usuario Registra Devolucion</strong></td>
  </tr>
<% for archivosprestamo in @archivosprestamos
        cadena = ""
        cadenae = ""
        @archivospersonas = Archivospersona.find_all_by_archivo_id(archivosprestamo.archivo_id);
        for archivospersona in @archivospersonas
          persona  = Persona.find(archivospersona.persona_id);
          if cadena != ""
            cadena = cadena + ' - ' + persona.autobuscar
          else
            cadena = persona.autobuscar
          end
        end
        @archivosempleados = Archivosempleado.find_all_by_archivo_id(archivosprestamo.archivo_id);
        for archivosempleado in @archivosempleados
          empleado  = Empleado.find(archivosempleado.empleado_id);
          if cadenae != ""
            cadenae = cadenae + ' - ' + empleado.autobuscar
          else
            cadenae = empleado.autobuscar
          end
        end
%>
  <tr valign="middle">
     <td width="5%" style="font-size: 11px; font-family: Tahoma;"><div align="left"><%=cadena rescue nil%></div></td>
     <td width="5%" style="font-size: 11px; font-family: Tahoma;"><div align="left"><%=cadenae rescue nil%></div></td>
     <td width="5%" style="font-size: 11px; font-family: Tahoma;"><div align="left"><%=archivosprestamo.archivo.caja rescue nil%></div></td>
     <td width="5%" style="font-size: 11px; font-family: Tahoma;"><div align="left"><%=archivosprestamo.archivo.carpeta rescue nil%></div></td>
     <td width="5%" style="font-size: 11px; font-family: Tahoma;"><div align="left"><%=archivosprestamo.archivo.manzana rescue nil%></div></td>
     <td width="5%" style="font-size: 11px; font-family: Tahoma;"><div align="left"><%=archivosprestamo.archivo.lote rescue nil%></div></td>
     <td width="5%" style="font-size: 11px; font-family: Tahoma;"><div align="left"><%=archivosprestamo.archivo.conse_inicial rescue nil%></div></td>
     <td width="5%" style="font-size: 11px; font-family: Tahoma;"><div align="left"><%=archivosprestamo.archivo.conse_final rescue nil%></div></td>
     <td width="5%" style="font-size: 11px; font-family: Tahoma;"><div align="left"><%=archivosprestamo.archivo.resol_inicial rescue nil%></div></td>
     <td width="5%" style="font-size: 11px; font-family: Tahoma;"><div align="left"><%=archivosprestamo.archivo.resol_final rescue nil%></div></td>
     <td width="5%" style="font-size: 11px; font-family: Tahoma;"><div align="left"><%=archivosprestamo.nro_folios  rescue nil%></div></td>
     <td width="5%" style="font-size: 11px; font-family: Tahoma;"><div align="left"><%=archivosprestamo.created_at  rescue nil%></div></td>
     <td width="5%" style="font-size: 11px; font-family: Tahoma;"><div align="left"><%=User.find(archivosprestamo.userregistro).nombre rescue nil%></div></td>
     <td width="5%" style="font-size: 11px; font-family: Tahoma;"><div align="left"><%=User.find(archivosprestamo.userprestamo).nombre rescue nil%></div></td>
     <td width="5%" style="font-size: 11px; font-family: Tahoma;"><div align="left"><%=archivosprestamo.fecha_vence  rescue nil%></div></td>
     <td width="5%" style="font-size: 11px; font-family: Tahoma;"><div align="left"><%=archivosprestamo.fecha_devolucion  rescue nil%></div></td>
     <td width="5%" style="font-size: 11px; font-family: Tahoma;"><div align="left"><%=User.find(archivosprestamo.userdevolucion).nombre rescue nil%></div></td>
 </tr>
<% end %>
</table>

