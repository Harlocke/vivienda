<table width="100%" border="0" cellpadding="2" cellspacing="2">
  <tr>
    <td height="30"><div class="col_enc">ASIGNACIONES ANTERIORES</div></td>
  </tr>
</table>
<table width="100%" border="0" cellpadding="0" cellspacing="0">
  <tr valign="middle">
    <td width="15%" height="20" class="oce-first">Elimina</td>
    <td width="15%" height="20" align="left" class="oce-first">Fecha Inicial</td>
    <td width="15%" height="20" align="left" class="oce-first">Fecha Final </td>
    <td width="55%" height="20" align="left" class="oce-first">Observaciones</td>
  </tr>
<% @vivienda.antviviendaspersonas.each do |antviviendaspersona| %>
   <tr  valign="middle" id="antviviendaspersona_<%=antviviendaspersona.id%>">
    <td class="camposDesc">
       <table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tr>
           <td class="camposDesc" align="left"><%=antviviendaspersona.fecha_desvincula.strftime("%Y-%m-%d %X") rescue nil%></td>
        </tr>
        <tr>
           <td class="camposDesc" align="left"><a title="<%=User.find(antviviendaspersona.user_desvincula).nombre rescue nil%>"><%=User.find(antviviendaspersona.user_desvincula).username rescue nil%></a></td>
        </tr>
      </table>
    </td>
     <td height="0" align="left" class="camposDesc"><%=antviviendaspersona.fecha_inicio.strftime("%Y-%m-%d %X") rescue nil%></td>
     <td height="0" align="left" class="camposDesc"><%=antviviendaspersona.fecha_final.strftime("%Y-%m-%d %X") rescue nil%></td>
     <td height="0" align="left" class="camposDesc"><%=antviviendaspersona.observaciones%></td>
 </tr>
<% end %>
</table>
