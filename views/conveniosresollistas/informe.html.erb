        <td width="90%" height="30" align="left" class="campos">DOCUMENTACION</td>
        <td width="10%" height="30" align="left" class="campos"><div align="center">ESTADO</div></td>
      </tr>
    <% @conveniosresolintencion.conveniosresollistas.each do |mejoramientoslista| %>
       <tr  valign="middle" class="<%=cycle('even1', 'odd1')%>">
         <td class="camposDesc"><div align="justify"><%=mejoramientoslista.listaspago.descripcion rescue nil%></div></td>
         <td align="left" class="camposDesc"><div align="center"><strong><%=mejoramientoslista.estado rescue nil%></strong></div></td>
        </tr>
     <% end %>
    </table>
    <br/>
    <table width="100%" border="1" cellspacing="0" cellpadding="0" class="sample">
      <tr class="odd1">
        <td width="30%" height="30" class="campos">Beneficiario</td>
        <td width="10%" class="campos">Cedula</td>
        <td width="5%" class="campos"><div align="center">Estado Mejo.</div></td>
        <td width="8%" class="campos"><div align="center">Vlr Subsidio</div></td>
        <td width="8%" class="campos"><div align="center">Pagos</div></td>
