        <td class="camposDesc"><%=@conveniosresolintencion.conveniosresolucion.convenio.tipo_solucion rescue nil%></td>
        <td class="camposDesc"><%=@conveniosresolintencion.conveniosresolucion.convenio.comuna.comuna rescue nil%></td>
      </tr>
    </table>
    <table width="100%" border="0" cellpadding="0" cellspacing="0">
      <tr class="odd1">
        <td width="42%" class="campos">Responsable Diagnostico</td>
        <td width="18%" class="campos">Valor Diagnostico</td>
        <td width="40%" class="campos">Operador</td>
      </tr>
      <tr>
        <td class="camposDesc"><%=@conveniosresolintencion.conveniosresolucion.convenio.responsable_diagnostico rescue nil%></td>
        <td class="camposDesc"><%=@conveniosresolintencion.conveniosresolucion.convenio.valor_diagnostico rescue nil%></td>
        <td class="camposDesc"><%=@conveniosresolintencion.conveniosresolucion.convenio.ejecutor rescue nil%></td>
      </tr>
    </table>
    <br/>
    <table width="100%" border="0" cellpadding="0" cellspacing="0">
      <tr>
        <td width="70%" height="50" class="campos odd1">Concepto de Visto Bueno </td>
        <td width="30%"><div class="col_enc" align="center">
          <% if @conveniosresolintencion.conveniosresollistas.exists?(["estado = 'NO'"]) %>
            NEGATIVO
          <% else %>
            POSITIVO
          <% end %></div>
        </td>
      </tr>
    </table>
    <br/>
