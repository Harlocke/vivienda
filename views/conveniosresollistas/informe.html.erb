      <tr>
        <td width="50%" height="50" class="campos"></td>
        <td width="50%" height="50" class="campos"></td>
      </tr>
      <tr class="odd1">
        <td height="0" class="campos">ASESOR JURIDICO UDEM</td>
        <td height="0" class="campos">ASESOR ADMINISTRATIVO UDEM</td>
      </tr>
    </table>
