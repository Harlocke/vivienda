        <td width="8%" class="campos"><div align="center">% Ejecucion</div></td>
        <td width="8%" class="campos"><div align="center">Vlr Aprobado</div></td>
        <td width="5%" class="campos"><div align="center">Estado</div></td>
      </tr>
    <% @conveniosresolintencion.conveniosresolmejoramientos.each do |conveniosresolmejoramiento| %>
       <tr  valign="middle" class="<%=cycle('even1', 'odd1')%>">
         <td  height="20" class="camposDesc"><div align="justify"><%=conveniosresolmejoramiento.mejoramiento.persona.nombres rescue nil%>
           <% if conveniosresolmejoramiento.observacion %>
             <br/><strong>NO OK : <%=conveniosresolmejoramiento.observacion rescue nil%></strong>
           <% end %></div></td>
         <td class="camposDesc"><div align="center"><%=conveniosresolmejoramiento.mejoramiento.persona.identificacion rescue nil%></div></td>
         <td class="camposDesc"><div align="center"><%=conveniosresolmejoramiento.mejoramiento.mejoramientosestado.descripcion rescue nil%></div></td>
         <td class="camposDesc"><div align="right">$<%=camponumerico(conveniosresolmejoramiento.mejoramiento.valor_resolucion) rescue 0%></div></td>
         <td class="camposDesc"><div align="right">$<%=camponumerico(conveniosresolmejoramiento.mejoramiento.mejoramientospagos.sum("valor")) rescue 0%></div></td>
         <td class="camposDesc"><div align="right">$<%=camponumerico(conveniosresolmejoramiento.valor_cobrado) rescue 0%></div></td>
         <td class="camposDesc"><div align="right"><%=conveniosresolmejoramiento.mejoramiento.porcentajeeje rescue 0%>%</div></td>
         <td class="camposDesc"><div align="right">$<%=camponumerico(conveniosresolmejoramiento.valor) rescue 0%></div></td>
         <td class="camposDesc"><div align="center"><%=conveniosresolmejoramiento.destado rescue nil%></div></td>
       </tr>
     <%end%>
       <tr class="odd1">
         <td  height="30" class="camposDesc" colspan="7"><strong>T O T A L</strong></td>
         <td class="camposDesc" colspan="2"><strong><div align="center">$ <%=camponumerico(@conveniosresolintencion.conveniosresolmejoramientos.sum("valor")) rescue nil%></div></strong></td>
       </tr>
