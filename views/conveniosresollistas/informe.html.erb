    <table width="100%" height="0"border="0" cellpadding="2" cellspacing="2"  align="center">
      <tr><td height="0"class="campos"> S I F I - Sistema de Informacion ISVIMED - <%=Time.now().strftime("%Y-%m-%d %H:%M:%S")%></td></tr>
    </table>
</td>
</tr>
</table>
</div>
