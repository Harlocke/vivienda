          <td width="33%" align="left" class="campos">Coordinador Operador</td>
          <td width="33%" align="left" class="campos">Ingeniero Operador</td>
          <td width="33%" align="left" class="campos">Profesional Operador</td>
        </tr>
        <tr>
          <td class="camposDesc"><%=@conveniosresolintencion.conveniosresolucion.responsable1 rescue nil rescue nil%>&nbsp;</td>
          <td class="camposDesc"><%=@conveniosresolintencion.conveniosresolucion.responsable2 rescue nil rescue nil%>&nbsp;</td>
          <td class="camposDesc"><%=@conveniosresolintencion.conveniosresolucion.responsable3 rescue nil rescue nil%>&nbsp;</td>
        </tr>
      </table>
      <% end  %>
      <table width="100%" border="0" cellpadding="0" cellspacing="0">
      <tr class="odd1">
        <td width="15%" class="campos">Nro Convenio</td>
        <td width="15%" class="campos">Vigencia</td>
        <td width="15%" class="campos">Nro Soluciones</td>
        <td width="20%" class="campos">Tipo Solucion</td>
        <td width="35%" class="campos">Ubicacion</td>
      </tr>
      <tr>
        <td class="camposDesc"><%=@conveniosresolintencion.conveniosresolucion.convenio.nro_convenio rescue nil%></td>
        <td class="camposDesc"><%=@conveniosresolintencion.conveniosresolucion.convenio.vigencia rescue nil%></td>
