<table width="100%" height="40"  align="center">
      <tr>
        <td>
          <table width="100%" height="100%" border="0" cellpadding="0" cellspacing="0"  align="center">
            <tr>
              <td width="3%"><img src="/images/titulacionmin.png" width="64" height="64"/></td>
              <td width="97%"><div class="col_enc">&nbsp;&nbsp;OBSERVACIONES A PERSONAS VINCULADAS</div></td>
            </tr>
          </table></td>
      </tr>
</table>
<br/>
<table width="95%" border="0" align="center" cellpadding="0" cellspacing="0">
  <tr valign="middle">
    <td width="50%" height="30" align="left" class="oce-first">Persona Vinculada</td>
    <td width="10%" height="30" align="left" class="oce-first">Nro Matricula</td>
    <td width="30%" height="30" align="left" class="oce-first">Direccion</td>
  </tr>
   <tr  valign="middle">
     <td height="30" align="left" class="camposDesc"><%=@titulacionespersona.persona.autobuscar rescue nil%></td>
     <td class="camposDesc"><%=@titulacionespersona.matricula rescue nil%></td>
     <td class="camposDesc"><%=@titulacionespersona.direccion rescue nil%></td>
   </tr>
</table>
<br/>
<% form_for(:titulacionespersonasnota, @titulacionespersonasnota, :url => nota_titulacionespersonasnotas_path(@titulacionespersona)) do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
<table width="100%" border="0" cellpadding="2" cellspacing="2">
  <tr>
    <td  height="30"><div align="right">
          <div class="uiButton"><img class="img" src="/images/guardar.png"/>&nbsp;&nbsp;<span class="uiButtonText"><%= submit_tag "Guardar", :disable_with => "Guardando..."%></span></div>
          <div class="uiButton"><img class="img" src="/images/cancel.png"/>&nbsp;&nbsp;<span class="uiButtonText"><%= link_to 'Regresar',  edit_titulacion_path(@titulacionespersona.titulacion_id) %></span></div>
    </div></td>
  </tr>
</table>
<% end %>

