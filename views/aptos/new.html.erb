<h1>New apto</h1>

<% form_for(@apto) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :piso_id %><br />
    <%= f.text_field :piso_id %>
  </p>
  <p>
    <%= f.label :descripcion %><br />
    <%= f.text_field :descripcion %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', aptos_path %>