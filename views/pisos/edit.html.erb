<h1>Editing piso</h1>

<% form_for(@piso) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :bloque_id %><br />
    <%= f.text_field :bloque_id %>
  </p>
  <p>
    <%= f.label :descripcion %><br />
    <%= f.text_field :descripcion %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @piso %> |
<%= link_to 'Back', pisos_path %>