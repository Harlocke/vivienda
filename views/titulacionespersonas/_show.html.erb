<fieldset>
<table width="100%" border="0" cellpadding="2" cellspacing="2">
  <tr>
    <td height="30"><div class="col_enc">INFORMACION TITULACION</div></td>
  </tr>
</table>
  <br/>
<% @persona.titulacionespersonas.each do |titulacionespersona| %>
    <% if titulacionespersona.id != @titulacionespersona.id %>
  <div align="center">
<table width="80%" align="center" id="one-column-emphasis">
  <tr class="odd1">
    <td width="20%" class="campos" align="left"><div align="left">Id Titulacion</div></td>
    <td width="80%" align="left" class="camposDesc"><div align="left"><%=titulacionespersona.titulacion.id rescue nil%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">Comuna</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=titulacionespersona.titulacion.cobama rescue nil%></div></td>
  </tr>
  <tr class="odd1">
    <td class="campos" align="left"><div align="left">Barrio</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=titulacionespersona.titulacion.barrio rescue nil%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">Manzana</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=titulacionespersona.titulacion.manzana rescue nil%></div></td>
  </tr>
  <tr class="odd1">
    <td class="campos" align="left"><div align="left">Lote</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=titulacionespersona.titulacion.lote rescue nil%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">Interior</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=titulacionespersona.titulacion.interior rescue nil%></div></td>
  </tr>
  <tr class="odd1">
    <td class="campos" align="left"><div align="left">Sector</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=titulacionespersona.titulacion.sector rescue nil%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">Direccion</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=titulacionespersona.titulacion.direccion rescue nil%></div></td>
  </tr>
  <tr class="odd1">
    <td class="campos" align="left"><div align="left">Estado del Proceso</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=titulacionespersona.titulacion.estado rescue nil%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">Informes</div></td>
    <td align="left" class="camposDesc"><div align="left">
        <%= link_to 'Seguimiento', {:controller=>"titulaciones", :action=>"seguimiento", :id=>titulacionespersona.titulacion.id}, :popup => ['windownew','height=520,width=830,scrollbars=yes'], :class=>"verlink"%>
- <%= link_to 'Seguimiento Reconocimiento', {:controller=>"titulaciones", :action=>"seguimientoreco", :id=>titulacionespersona.titulacion.id}, :popup => ['windownew','height=520,width=830,scrollbars=yes'], :class=>"verlink"%>
- <%= link_to 'Seguimiento Santa Elena', {:controller=>"titulaciones", :action=>"seguimientoelena", :id=>titulacionespersona.titulacion.id}, :popup => ['windownew','height=520,width=830,scrollbars=yes'], :class=>"verlink"%>
- <%= link_to 'Informe POT', {:controller=>"titulaciones", :action=>"seguimientopot", :id=>titulacionespersona.titulacion.id}, :popup => ['windownew','height=520,width=830,scrollbars=yes'], :class=>"verlink"%>
      </div></td>
  </tr>
  <% if titulacionespersona.titulacion.titulacionesdemandas.exists? %>
  <tr class="odd1">
    <td class="campos" align="left"><div align="left">Demanda</div></td>
    <td align="left" class="camposDesc"><div align="left">
<table width="100%" border="0" align="center" cellpadding="0" cellspacing="0">
  <tr valign="middle">
    <td width="20%" height="20" class="oce-first">Radicado</td>
    <td width="20%" class="oce-first">Juzgado</td>
    <td width="20%" class="oce-first">Estado</td>
    <td width="30%" class="oce-first">Abogado</td>
  </tr>
    <% titulacionespersona.titulacion.titulacionesdemandas.each do |titulacionesdemanda| %>
   <tr valign="middle" class="<%=cycle('even1', 'odd1')%>" id="titulacionesdemanda_<%= titulacionesdemanda.id%>">
     <td height="30" class="camposDesc"><%=titulacionesdemanda.radicado rescue nil%></td>
     <td class="camposDesc"><%=titulacionesdemanda.juzgado rescue nil%></td>
     <td class="camposDesc"><%=titulacionesdemanda.estado rescue nil%></td>
     <td class="camposDesc"><%=User.find(titulacionesdemanda.user_juridico).nombre rescue nil%></td>
   </tr>
   <% end %>
   </table>
      </div></td>
  </tr>
  <% end %>
  <tr>
    <td class="campos" align="left">&nbsp;</td>
    <td align="left" class="camposDesc">
       <% if permiso("titulacion","C").to_s == "S" or permiso("titulacionesconsulta","C").to_s == "S" %>
          <div align="right">
            <div class="uiButton"><img class="img" src="/images/ver3.png"/>&nbsp;&nbsp;<span class="uiButtonText"><%= link_to 'Ver Informacion', edit_titulacion_path(titulacionespersona.titulacion_id) %></span></div>
          </div>
       <% end %>
    </td>
  </tr>
</table>
<table width="80%" height="0" border="0" cellpadding="0" cellspacing="0">
<tr>
  <td height="10"><hr size="1" noshade class="hrlinea"/></td>
</tr>
</table>
    <br/>
     <% end %>
  </div>
<% end %>
</fieldset>
