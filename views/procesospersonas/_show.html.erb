<fieldset>
<table width="100%" border="0" cellpadding="2" cellspacing="2">
  <tr>
    <td height="30"><div class="col_enc">INFORMACION PROCESOS JURIDICOS</div></td>
  </tr>
</table>
  <br/>
<% @persona.procesospersonas.each do |procesospersona| %>
    <% if procesospersona.id != @procesospersona.id %>
  <div align="center">
<table width="70%" align="center" id="one-column-emphasis">
  <tr>
    <td width="30%" class="campos" align="left"><div align="left">Tipo Proceso</div></td>
    <td width="70%" align="left" class="camposDesc"><div align="left"><%=procesospersona.procesosjuridico.tiposproceso.descripcion rescue nil%></div></td>
  </tr>  
  <tr>
    <td class="campos" align="left"><div align="left">Nro Radicado</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=procesospersona.procesosjuridico.nro_radicado%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">AÃ±o</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=procesospersona.procesosjuridico.anno_radicado%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">Juzgado</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=procesospersona.procesosjuridico.juzgado%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">Demandado</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=procesospersona.procesosjuridico.demandado%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">Tema</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=procesospersona.procesosjuridico.etapa%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">SubTema</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=procesospersona.procesosjuridico.subetapa%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">Poblacion Especial</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=procesospersona.procesosjuridico.poblacionesespecial.descripcion rescue nil%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">Observaciones Proceso </div></td>
    <td align="left" class="camposDesc"><div align="left"><%=procesospersona.procesosjuridico.observacion_proceso%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">Pretencion</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=procesospersona.procesosjuridico.pretension%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left"><div align="left">Cuantia</div></td>
    <td align="left" class="camposDesc"><div align="left"><%=procesospersona.procesosjuridico.cuantia%></div></td>
  </tr>
  <tr>
    <td class="campos" align="left">&nbsp;</td>
    <td align="left" class="camposDesc">
       <% if permiso("procesosjuridico","C").to_s == "S" %>
          <div align="right">
            <div class="uiButton"><img class="img" src="/images/ver3.png"/>&nbsp;&nbsp;<span class="uiButtonText"><%= link_to 'Ver Informacion', edit_procesosjuridico_path(procesospersona.procesosjuridico_id) %></span></div>
          </div>
       <% end %>
    </td>
  </tr>
</table>
<table width="70%" height="0" border="0" cellpadding="0" cellspacing="0">
<tr>
  <td height="10"><hr size="1" noshade class="hrlinea"/></td>
</tr>
</table>
    <br/>
     <% end %>
  </div>
<% end %>
</fieldset>
