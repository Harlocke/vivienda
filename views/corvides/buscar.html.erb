<table width="100%" height="40">
  <tr>
    <td class="camposDesc">
      <table width="100%" height="100%" border="0" cellpadding="0" cellspacing="0">
        <tr>
          <td width="3%"><img src="/images/corvidemin.png" width="64" height="64"/></td>
          <td width="94%"><div class="col_enc">CORVIDE</div></td>
          <td width="3%"><div align="right"><%= link_to image_tag('atras.png',{ :border=>0, :title => "Buscar"}),corvides_path %>
          </div></td>
        </tr>
      </table></td>
  </tr>
</table>
<div align="center">
<table width="90%" border="0" align="center" cellpadding="0" cellspacing="0">
  <tr valign="middle">
    <td width="13%" height="30" class="oce-first">Nro. Expediente</td>
    <td width="17%" height="30" class="oce-first">Matr√≠cula Inmobiliaria</td>
    <td width="15%" height="30" class="oce-first">CBML</td>
    <td width="25%" height="30" class="oce-first">Proyecto</td>
    <td width="20%" height="30" class="oce-first">Estado</td>
    <td width="10%" class="oce-first">&nbsp;</td>
  </tr>
<% @corvides.each do |corvide| %>
  <tr class="<%=cycle('even1', 'odd1')%>">
    <td class="camposDesc"><%=h corvide.nro_expediente rescue nil %></td>
    <td class="camposDesc"><%=h corvide.matricula rescue nil %></td>
    <td class="camposDesc"><%=h corvide.cbml rescue nil %></td>
    <td class="camposDesc"><%=h corvide.transferencia rescue nil %></td>
    <td class="camposDesc"><%=h corvide.estado rescue nil %></td>
    <td class="camposDesc" ><div align="center"><%= link_to 'Editar', edit_corvide_path(corvide), :class=>"editarlink"%>
   <% if permiso("corvide","E").to_s == "S" %>
    <%= link_to 'borrar', corvide_path(corvide),
                          :confirm => 'Esta Seguro de Eliminar?',:method => :delete, :class=>"borrarlink" %>
    <% end %></div></td>
  </tr>
<% end %>
</table>
<br/>
<table width="90%" height="40" border="0" cellpadding="0" cellspacing="0">
   <tr>
     <td width="100%" class="acci_titcontenido2"><div align="center">Total Registros: <%= @corvides.count %></div></td>
   </tr>
</table>
<br/>
</div>