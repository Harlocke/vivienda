<h1>Listing tipospoblaciones</h1>

<table>
  <tr>
    <th>Descripcion</th>
  </tr>

<% @tipospoblaciones.each do |tipospoblacion| %>
  <tr>
    <td><%=h tipospoblacion.descripcion %></td>
    <td><%= link_to 'Show', tipospoblacion %></td>
    <td><%= link_to 'Edit', edit_tipospoblacion_path(tipospoblacion) %></td>
    <td><%= link_to 'Destroy', tipospoblacion, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New tipospoblacion', new_tipospoblacion_path %>