<h1>Editing agendasrango</h1>

<% form_for(@agendasrango) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :agendasproceso %><br />
    <%= f.text_field :agendasproceso %>
  </p>
  <p>
    <%= f.label :user %><br />
    <%= f.text_field :user %>
  </p>
  <p>
    <%= f.label :fecha_inicio %><br />
    <%= f.date_select :fecha_inicio %>
  </p>
  <p>
    <%= f.label :fecha_fin %><br />
    <%= f.date_select :fecha_fin %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @agendasrango %> |
<%= link_to 'Back', agendasrangos_path %>