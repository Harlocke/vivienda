   <%= stylesheet_link_tag 'scaffold'%>
   <%=  javascript_include_tag :defaults %>
<table width="100%" height="40">
      <tr>
        <td>
          <table width="100%" height="100%" border="0" cellpadding="0" cellspacing="0">
            <tr>
              <td width="3%"><img src="/images/inmueblesmin.png" width="64" height="64"/></td>
              <td width="94%"><div class="col_enc">INVENTARIOS DE INMUEBLES</div></td>
              <td width="3%"><div align="right"><%= link_to image_tag('atras.png',
	            { :border=>0, :title => "Buscar"}),busqueda_almacenesinmuebles_path %>
                </div></td>
            </tr>
          </table></td>
      </tr>
</table>
<br/>
<table width="90%" border="0" align="center" cellpadding="0" cellspacing="0">
  <tr valign="middle">
    <td width="10%" height="30" class="oce-first"><div align="center">Matricula</div></td>
    <td width="25%" height="30" class="oce-first"><div align="center">Ubicacion</div></td>
    <td width="25%" class="oce-first"><div align="center">Direccion Catastro</div></td>
    <td width="25%" class="oce-first"><div align="center">Estado</div></td>
    <td width="10%" class="oce-first"><div align="center">Avaluo</div></td>
    <td width="5%" class="oce-first">&nbsp;</td>
  </tr>
     <% i = 0 %>
<% @almacenesinmuebles.each do |almacenesinmueble| %>
    <% i = i + 1 %>
<tr valign="middle" class="<%=cycle('even1', 'odd1')%>">
    <td height="30" class="camposDesc"><div align="center"><%=h almacenesinmueble.matricula%></div></td>
    <td height="30" class="camposDesc"><%=h almacenesinmueble.lugar%></td>
    <td height="30" class="camposDesc"><%=h almacenesinmueble.direccion_catastro%></td>
    <td class="camposDesc"><%=h almacenesinmueble.estado%></td>
    <td class="camposDesc"><div align="center"><%=h almacenesinmueble.avaluo%></div></td>
    <td class="camposDesc"><div align="center">
    <%= link_to 'Editar', edit_almacenesinmueble_path(almacenesinmueble),:class=>"editarlink"%>
      </div></td>
  </tr>
<% end %>
</table>
<table width="80%" height="0" align="center" border="0" cellpadding="0" cellspacing="0">
  <tr>
    <td height="10"><hr size="1" noshade class="hrlinea"/></td>
  </tr>
</table>
<table width="80%" border="0" align="center" cellpadding="0" cellspacing="0">
  <tr valign="middle">
    <td width="100%" height="30" class="camposDescN"><div align="center">Nro Total de Registro : <%=i%></div></td>
  </tr>
</table>