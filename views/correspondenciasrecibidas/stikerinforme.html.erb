<table width="100%" height="40">
      <tr>
        <td>
          <table width="100%" height="100%" border="0" cellpadding="0" cellspacing="0">
            <tr>
              <td width="3%"><img src="/images/correspondenciarecibidamin.png" width="64" height="64"/></td>
              <td width="85%"><div class="col_enc">CORRESPONDENCIA RECIBIDA</div></td>
            </tr>
          </table></td>
      </tr>
</table>
<br/>
<table width="80%" border="0" cellpadding="2" cellspacing="2"  align="center">
  <tr><td></td></tr>
  <tr>
    <td align="left" height="30"><div class="col_enc">GENERACION DE STIKER</div></td>
  </tr>
</table>
<% form_tag stikerp_correspondenciasrecibidas_path, :method => 'get' do %>
<div align="center">
<table width="70%" border="0" cellspacing="2" cellpadding="0">
    <tr>
          <td width="33%" height="20" class="campos" align="left">Rango de Radicados</td>
          <td class="camposDesc" align="left">Inicial &nbsp;<%= text_field_tag :inicial, params[:inicial],{ :class => "campo", :size => 15,
                  :onChange=> "javascript:this.value=this.value.toUpperCase();",
                :onFocus => "this.style.border='1px solid #f00'",
                :onBlur => "this.style.border='1px solid #bbb'" } %>&nbsp;&nbsp;Final&nbsp;
          <%= text_field_tag :final, params[:final],{ :class => "campo", :size => 15,
                  :onChange=> "javascript:this.value=this.value.toUpperCase();",
                :onFocus => "this.style.border='1px solid #f00'",
                :onBlur => "this.style.border='1px solid #bbb'" } %></td>
        </tr>
    <tr>
      <td width="33%" height="20" class="campos" align="left">Cantidad</td>
      <td class="camposDesc" align="left"><%= text_field_tag :cantidad, params[:cantidad],{ :class => "campo", :size => 10,
                  :onChange=> "javascript:this.value=this.value.toUpperCase();",
                :onFocus => "this.style.border='1px solid #f00'",
                :onBlur => "this.style.border='1px solid #bbb'" } %></td>
    </tr>
</table>
<table width="80%" height="0" border="0" align="center" cellpadding="0" cellspacing="0">
      <tr>
        <td height="10"><hr size="1" noshade class="hrlinea"/></td>
      </tr>
</table>
<table width="80%" border="0" cellpadding="2" align="center" cellspacing="2">
  <tr>
    <td  height="30"><div align="right">
       <div class="uiButton"><img class="img" src="/images/excel.png"/>&nbsp;&nbsp;<span class="uiButtonText">
          <%= submit_tag "Generar"%>
        </span></div>
</div></td>
  </tr>
</table>
</div>
<% end %>
<br/>
