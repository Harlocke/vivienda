<table width="100%" height="40">
      <tr>
        <td>
          <table width="100%" height="100%" border="0" cellpadding="0" cellspacing="0">
            <tr>
              <td width="3%"><img src="/images/archivomin.png" width="64" height="64"/></td>
              <td width="84%"><div class="col_enc">ARCHIVOS DOCUMENTOS</div></td>
              <td width="12%"><div align="right">
                  <%= link_to image_tag('nuevo.png',
	                            { :border=>0, :title => "Nuevo"}),new_archivosdocumento_path %>
                  <%= link_to image_tag('atras.png',
	            { :border=>0, :title => "Buscar"}),archivos_path %>
                </div></td>
            </tr>
          </table></td>
      </tr>
</table>
<table width="80%" border="0" align="center" cellpadding="0" cellspacing="0">
  <tr>
    <td width="90%" height="30" class="oce-first">Descripcion</td>
    <td width="10%" height="30" class="oce-first"></td>
  </tr>
<% @archivosdocumentos.each do |archivosdocumento| %>
  <tr valign="middle" class="<%=cycle('even1', 'odd1')%>">
    <td class="camposDesc"><%=h archivosdocumento.descripcion %></td>
    <td class="camposDesc"><div align="center">
        <% if permiso("archivosdocumento","A").to_s == "S" %>
        <%= link_to 'Editar', edit_archivosdocumento_path(archivosdocumento) , :class=>"editarlink"%>
        <% if Archivosseriesdoc.exists?(["archivosdocumento_id = #{archivosdocumento.id}"]) == false %>
        &nbsp;|&nbsp;<%= link_to 'Borrar', archivosdocumento_path(archivosdocumento), :confirm => 'Esta Seguro de Eliminar?',:method => :delete, :class=>"borrarlink" %>
        <% end %>
        <% end %>
      </div></td>
        
  </tr>
<% end %>
</table>
<br/>