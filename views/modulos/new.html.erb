<h1>New modulo</h1>

<% form_for(@modulo) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :descripcion %><br />
    <%= f.text_field :descripcion %>
  </p>
  <p>
    <%= f.label :ruta %><br />
    <%= f.text_field :ruta %>
  </p>
  <p>
    <%= f.label :mensaje %><br />
    <%= f.text_field :mensaje %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', modulos_path %>