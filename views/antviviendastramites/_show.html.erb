<table width="100%" border="0" cellpadding="2" cellspacing="2">
  <tr>
    <td height="30"><div class="col_enc">TRAMITES ANTERIORES</div></td>
  </tr>
</table>
<table width="100%" border="0" cellpadding="0" cellspacing="0">
  <tr valign="middle">
    <td width="20%" height="20" class="oce-first">Elimina</td>
    <td width="25%" height="20" align="left" class="oce-first">Fecha</td>
    <td width="30%" height="20" align="left" class="oce-first">Actividad</td>
    <td width="10%" height="20" align="left" class="oce-first"><div align="center">Fecha <br>Inicio</div></td>
    <td width="10%" height="20" align="left" class="oce-first"><div align="center">Fecha <br>Fin</div></td>
    <td width="5%" height="20" align="left" class="oce-first"><div align="center">Tiempo<br>Max.</div></td>
  </tr>
<% @vivienda.antviviendastramites.each do |antviviendastramite| %>
   <tr  valign="middle" id="antviviendastramite_<%=antviviendastramite.id%>">
    <td class="camposDesc">
       <table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tr>
           <td class="camposDesc" align="left"><%=antviviendastramite.fecha_desvincula.strftime("%Y-%m-%d %X") rescue nil%></td>
        </tr>
        <tr>
           <td class="camposDesc" align="left"><a title="<%=User.find(antviviendastramite.user_desvincula).nombre rescue nil%>"><%=User.find(antviviendastramite.user_desvincula).username rescue nil%></a></td>
        </tr>
      </table>
    </td>
<td height="0" align="left" class="camposDesc">
        <table width="100%" border="0" cellspacing="0" cellpadding="0">
          <tr>
             <td class="camposDesc" align="left"><a title="Creacion"><%=antviviendastramite.created_at.strftime("%Y-%m-%d %X")%>
             &nbsp;<%=User.find(antviviendastramite.user_id).username rescue nil%></a></td>
          </tr>
          <tr>
             <td class="camposDesc" align="left"><a title="Ultima Actualizacion"><%=antviviendastramite.updated_at.strftime("%Y-%m-%d %X")%>
             &nbsp;<%=User.find(antviviendastramite.useract_id).username rescue nil%></a></td>
          </tr>
        </table>
     </td>
     <td height="0" align="left" class="camposDesc">
        <table width="100%" border="0" cellspacing="0" cellpadding="0">
          <tr>
             <td class="camposDesc" align="left"><%=antviviendastramite.viviendastipostramite.etapa.slice!(0..8) rescue nil%></td>
          </tr>
          <tr>
             <td class="camposDesc" align="left"><%=antviviendastramite.viviendastipostramite.descripcion rescue nil%></td>
          </tr>
        </table>
     </td>
     <td height="0" align="left" class="camposDesc"><div align="center"><%=antviviendastramite.fecha_inicio%></div></td>
     <td height="0" align="left" class="camposDesc"><div align="center"><%=antviviendastramite.fecha_esperada%></div></td>
     <td height="0" align="left" class="camposDesc"><div align="center"><%=antviviendastramite.viviendastipostramite.tiempo rescue nil%></div></td>
 </tr>
 <% if antviviendastramite.observaciones.to_s != "" %>
   <tr  valign="middle">
     <td height="0" colspan="6" align="left" class="camposDesc"><table width="800" border="0" cellspacing="0" cellpadding="0">
       <tr>
         <td width="15%" class="oce-first">Observacion:</td>
         <td width="85%" class="campos"><%=antviviendastramite.observaciones%></td>
       </tr>
     </table></td>
    </tr>
    <% end %>
<% end %>
</table>
